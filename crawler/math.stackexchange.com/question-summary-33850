((x_1\otimes x_2)(x'_1\otimes x'_2))(x''_1\otimes x''_2)  = v^{|x_2||x'_1|}(x_1x'_1\otimes x_2x'_2) \otimes (x''_1\otimes x''_2)
= v^{|x_2||x'_1|+|x_2x'_2||x''_1|}x_1x'_1x''_1\otimes x_2x'_2x''_2
(x_1\otimes x_2)((x'_1\otimes x'_2)(x''_1\otimes x''_2))
((x_1\otimes x_2)(x'_1\otimes x'_2))(x''_1\otimes x''_2) = (x_1\otimes x_2)((x'_1\otimes x'_2)(x''_1\otimes x''_2))
|x_2|\cdot|x_1'|+|x_2x_2'|\cdot|x_1''|
|x_2'|\cdot|x_1''|+|x_2|\cdot|x_1'x_1''|
|xy|=|x|+|y|
'\mathbf f_\nu'\mathbf f_{\nu'}\subset'\mathbf f_{\nu+\nu'}
 \begin{eqnarray} && |x_2|\cdot|x_1'|+|x_2x_2'|\cdot|x_1''| \\ &=& |x_2|\cdot|x_1'|+(|x_2|+|x_2'|)\cdot|x_1''| \\ &=& |x_2|\cdot|x_1'|+|x_2|\cdot|x_1''|+|x_2'|\cdot|x_1''| \\ &=& |x_2'|\cdot|x_1''|+|x_2|\cdot|x_1'|+|x_2|\cdot|x_1''| \\ &=& |x_2'|\cdot|x_1''|+|x_2|\cdot(|x_1'|+|x_1''|) \\ &=& |x_2'|\cdot|x_1''|+|x_2|\cdot|x_1'x_1''|\;. \\ \end{eqnarray} 
