f(x), g(x)
x \in \mathbb{R}
f(x) = \sin x, g(x) = \cos x
f(x), g(x)
f'(x) = g(x), f(0) = 0, g'(x) = -f(x), g(0) = 1
f(x + y) = f(x)g(y) + g(x)f(y), g(x + y) = g(x)g(y) - f(x)f(y)
(f,g)
f^2 + g^2
 \dfrac{d}{dx} \left(f(x)^2 + g(x)^2\right) = 2 f(x) g(x) - 2 g(x) f(x) = 0 
(f_1, g_1)
(f_2, g_2)
f_1(0) = f_2(0)
g_1(0) = g_2(0)
f_3 = f_1 - f_2, g_3 = g_1 - g_2
f_3(0) = g_3(0) = 0
f_3(x)^2 + g_3(x)^2 = 0
f_3(x) = g_3(x) = 0
A(y) = f(x+y) - f(x) g(y) - g(x) f(y)
B(y) = g(x+y) -g(x) g(y) + f(x) f(y)
A(0) = B(0) = 0
 A'(y) = B(y)
B'(y) = -A(y)
A(y) = 0
B(y) = 0
