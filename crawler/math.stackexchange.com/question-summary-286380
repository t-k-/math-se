\langle a,b \rangle = \{\{a\},\{a,b\}\}
d=a\lor d=b
(b=a\land d=a)\lor b=d
b=d
\langle a,b\rangle=\langle c,d\rangle\iff a=c\land b=d.
\begin{align}&\langle a,b\rangle=\langle c,d\rangle\\ \implies&\{\{a\},\{a,b\}\}=\{\{c\},\{c,d\}\}\\ \implies&\{a\}\in\{\{c\},\{c,d\}\}\\ \implies&\{a\}=\{c\}\lor\{a\}=\{c,d\}\\ \implies&a=c\lor a=c=d\\ \implies&a=c\\ \end{align}
\begin{align}&\langle a,b\rangle=\langle a,d\rangle\\ \implies&\{\{a\},\{a,b\}\}=\{\{a\},\{a,d\}\}\\ \implies&\{a,b\}\in\{\{a\},\{a,d\}\}\\ \implies&\{a,b\}=\{a\}\lor \{a,b\}=\{a,d\}\\ \implies& b\in\{a\}\lor b\in\{a,d\}\\ \implies & b=a\lor b=d \end{align}
\langle a,b\rangle=\langle c,d\rangle\implies a=c\land b=d.
\langle b,a\rangle=\{{\{{b\}},\{{a,b\}}\}}
\langle a,b\rangle\neq\langle b,a\rangle
