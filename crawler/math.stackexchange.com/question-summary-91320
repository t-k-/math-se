\liminf \rho(x) \leqslant 1
 \pi(t) \geqslant (1+ \delta) \frac{t}{\ln t}, \tag{$\ast$} 
 \begin{align*} \int_2^x \frac{\pi(t)}{t^2} \, dt  & \geqslant \int_T^x \frac{\pi(t)}{t^2} \, dt  \\ &\stackrel{(\ast)}{\geqslant} \int_T^x (1 + \delta) \frac{t}{\ln t} \cdot \frac{1}{t^2}  \, dt \\ &=(1 + \delta) \int_T^x  \frac{1}{t \, \ln t}   \, dt \\ &= (1 + \delta) \cdot \left. \ln \ln t \right\vert_{t=T}^{t=x} \\ &= (1 + \delta) \cdot (\ln \ln x - \ln \ln T) \\ &\sim (1 + \delta) \cdot \ln \ln x, \end{align*} 
