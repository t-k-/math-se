||a| - |b|| \leq |a - b|
|b|
|a| - |b| \geq 0
|a| - |b| < 0
|a| - |b| < 0
|a| = |\left(a - b\right) + b| \leq |a - b| + |b|
|a| - |b| \leq |a - b|
|a| - |b| \geq 0 \implies ||a| - |b|| = |a| - |b| \leq |a - b|
|a| - |b| < 0
|b| - |a| \leq |b - a| = |a - b|
||a| - |b|| = -\left(|a| - |b|\right) = |b| - |a| \leq |a - b|
|a| - |b| \leq |a - b|
|b| - |a| \leq |b - a|
|b-a| = |a-b|
-1
-|a - b| \leq |a| - |b|
-|a-b| \leq |a| - |b| \leq |a - b|
 ||a|-|b|| \leq  |a-b| 
