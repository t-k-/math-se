 \begin{align} \int_0^1\frac{-1}{\log(x)+\log(1-x)}e^{-2\pi ix\xi}\;\mathrm{d}x &=\int_0^\infty\frac{-1}{\log(-is)+\log(1+is)}e^{-2\pi s\xi}(-i)\mathrm{d}s\\ &-\int_0^\infty\frac{-1}{\log(1-is)+\log(is)}e^{-2\pi i\xi-2\pi s\xi}(-i)\mathrm{d}s\\ &=\frac{1}{\xi}\int_0^\infty\frac{-1}{\log(-is/\xi)+\log(1+is/\xi)}e^{-2\pi s}(-i)\mathrm{d}s\\ &-\frac{e^{-2\pi i\xi}}{\xi}\int_0^\infty\frac{-1}{\log(1-is/\xi)+\log(is/\xi)}e^{-2\pi s}(-i)\mathrm{d}s\\ &\sim \frac{1-e^{-2\pi i\xi}}{2\pi i\xi\log|\xi|}\\ &=e^{-\pi i\xi}\frac{\sin(\pi \xi)}{\pi \xi\log|\xi|} \end{align} 
