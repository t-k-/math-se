 \begin{align} \int_{I_3}\left|\frac{e^{-2\pi s}}{\log(\xi)}\right|\mathrm{d}s &=\frac{e^{-2\pi\xi^\epsilon}}{2\pi\log(\xi)}\\ &=\frac{\epsilon e^{-2\pi/\epsilon+\log(1/\epsilon)}}{2\pi\log(\xi)}\\ &\le\frac{\epsilon}{2\pi\log(\xi)}\tag{7} \end{align} 
 \begin{align} \left|\frac{\log(-is)+\log(1+is/\xi)}{\log(\xi)}\right| &\le\left|\frac{\log(s)}{\log(\xi)}\right|+\left|\frac{\log(-i)+\log(1+is/\xi)}{\log(\xi)}\right|\\ &\le\epsilon+\left(\frac{\pi}{2}+\frac{1}{2}\log(2)\right)\frac{\epsilon}{\log(3)}\\ &\le\frac{11}{4}\epsilon\tag{8} \end{align} 
 \begin{align} \int_{I_2}\left|\frac{\frac{\log(-is)+\log(1+is/\xi)}{\log(\xi)}}{1-\frac{\log(-is)+\log(1+is/\xi)}{\log(\xi)}}\right|\;\frac{e^{-2\pi s}}{\log(\xi)}\;\mathrm{d}s &\le\frac{\frac{11}{4}\epsilon}{1-\frac{11}{4}\epsilon}\frac{1}{2\pi\log(\xi)}\\ &\le\frac{33\epsilon}{2\pi\log(\xi)}\tag{9} \end{align} 
 \begin{align} \int_{I_3}\left|\frac{-1}{\log(-is/\xi)+\log(1+is/\xi)}\right|\;e^{-2\pi s}\;\mathrm{d}s &\le\frac{2}{\log(2)}\frac{e^{-2\pi\xi^\epsilon}}{2\pi}\\ &=\frac{2}{\log(2)}\frac{\epsilon e^{-2\pi/\epsilon+\log(1/\epsilon)+\log(\log(\xi))}}{2\pi\log(\xi)}\\ &=\frac{2}{\log(2)}\frac{\epsilon e^{-2\pi/\epsilon+2\log(1/\epsilon)+\log(\log(1/\epsilon))}}{2\pi\log(\xi)}\\ &\le\frac{\epsilon}{\pi\log(2)\log(\xi)}\tag{10} \end{align} 
 \begin{align} &\left|\int_0^\infty\frac{-1}{\log(-is/\xi)+\log(1+is/\xi)}\;e^{-2\pi s}\;\mathrm{d}s - \frac{1}{2\pi\log(\xi)}\right|\\ &\le(2+1+\frac{1}{2\pi}+\frac{33}{2\pi}+\frac{1}{\pi\log(2)})\frac{\epsilon}{\log(\xi)}\\ &\le\frac{6\epsilon}{\log(\xi)}\tag{11} \end{align} 
