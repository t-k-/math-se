n=3
f(x_1,x_2,x_3)=(x_1^2+ x_2^2+x_3^2)^{-1/2}
\partial_{x_1}^2= \frac{2x_1^2-x_2^2-x_3^2}{(x_1^2+x_2^2+x_3)^{5/2}}, \partial_{x_2}^2=  \frac{2x_2^2-x_1^2-x_3^2}{(x_1^2+x_2^2+x_3)^{5/2}}, \partial_{x_3}^2= \frac{2x_3^2-x_1^2-x_2^2}{(x_1^2+x_2^2+x_3)^{5/2}} 
n \leadsto n+1
u \neq 0
f(x_1, \dots , x_n, )=(x_1^2+ \dots + x_{n}^2 ) ^{1-(n/2)}=:u 
  (\partial_{x_1}^2 + \dots + \partial_{x_n}^2)u=0
(\partial_{x_1}^2 + \dots + \partial_{x_n}^2+\partial_{x+1}^2)u=\underbrace{(\partial_{x_1}^2 + \dots + \partial_{x_n}^2)u}_{=0}+\partial_{n+1}^2u=0 \\ \implies \partial_{n+1}^2 u =0 \implies \partial_{n+1}^2 =0, \text{ because $u \neq 0$} 
r(x)^2:= x_1^2 + \dots + x_n^2
f(x) = r(x)^{2-n}
\partial_{x_1}f = (2-n)r^{1-n} \partial_{x_1}r
