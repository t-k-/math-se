\forall a, b \in R, a + b \in R
\forall a, b, c \in R, (a + b) + c = a + (b + c)
\exists 0 \in R, : \forall a \in R, 0 + a = a + 0 = a
\forall a \in R, \exists b (= "-a") \in R : a + b = b + a = 0
\forall a, b \in R, a + b = b + a
\forall a, b \in R, a · b \in R
\forall a, b, c \in R, (a · b) · c = a · (b · c)
\exists 1 \in R : \forall a \in R, 1 · a = a · 1 = a
\forall a, b, c \in R, a · (b + c) = (a · b) + (a · c)
\forall a, b, c \in R, (a + b) · c = (a · c) + (b · c)
\forall a, b \in S, a + b \in S
\forall a, b \in S, a · b \in S
\forall a \in S, −a \in S
1 \in S
