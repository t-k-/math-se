\begin{align*} \lim_{x \to \ x_1}f(x)&=f (x_1)=L_1\\ \lim_{x \to \ x_2}f(x)&=f (x_2)=L_2\\ \lim_{x \to \ x_3}f(x)&=f (x_3)=L_3\\ &\vdots\\ \lim_{x \to \ x_n}f(x)&=f (x_n)=L_n\\ \end{align*}
