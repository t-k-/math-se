\begin{align*} (0.25)^{3-0.5x^2} &\leq 8\\ (3-0.5x^2) \ln(0.25) &\leq \ln(8)\\ -0.5x^2\ln\left(\frac{1}{4}\right) &\leq \ln(8) -3\ln\left(\frac{1}{4}\right)\\ -0.5x^2(-\ln(4)) &\leq \ln(8) + 3\ln(4)\\ x^2\ln(2) &\leq 3\ln(2)+6\ln(2)\\ x^2\ln(2) &\leq 9\ln(2)\\ x^2 &\leq 9. \end{align*}
