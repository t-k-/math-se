3*33 = 99
100 - 99 = 1
\begin{align} 2*\{100*3-3*[100-3*(100-\color{red}{3*33})]\} &= 2*\{100*3-3*[100-3*(\color{red}{100-99})]\} \\ &= 2*\{100*3-3*[100-\color{red}{3*(1)}]\} \\ &= 2*\{100*3-3*[\color{red}{100-3}]\} \\ &= 2*\{100*3-\color{red}{3*[97]}\} \\  &= 2*\{\color{red}{100*3}-291\} \\ &= 2*\{\color{red}{300-291}\} \\ &= \color{red}{2*\{9\}} \\ &= 18 \end{align} 
