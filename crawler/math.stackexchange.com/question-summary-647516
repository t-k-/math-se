a_i
b_i
 0 < x(t) < 1
 0 < y(t) < 1
 \begin{aligned}   \begin{aligned}  x'(t)  =   & \,   a_{1}(1-x(t))(1-y(t)) \, + \\   & \,   a_{2}(1-x(t))(y(t)) \, + \\   & \,   a_{3}x(t)(1-y(t)) \, + \\   & \,   a_{4}x(t)y(t) \quad   \end{aligned}   & \quad \quad \quad  \begin{aligned}  y'(t)  =    & \,   b_{1}(1-x(t))(1-y(t)) \, + \\    & \,   b_{2}(1-x(t))(y(t)) \, + \\    & \,   b_{3}x(t)(1-y(t)) \, + \\    & \,   b_{4}x(t)y(t) \quad    \end{aligned}   \end{aligned}  
