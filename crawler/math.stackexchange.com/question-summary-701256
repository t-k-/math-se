h(x) = f(x)*(g(b) - g(a)) - g(x)*(f(b) - f(a))
[a, b]
h(a) = h(b)
c \in (a, b)
h'(c) = 0
 h(a) = f(a)g(b) - f(a)g(a) - g(a)f(b) + g(a)f(a) = f(a)g(b) - f(b)g(a), 
 h(b) = f(b)*(g(b) - g(a)) - g(b)*(f(b) - f(a)) = f(a)g(b) - f(b)g(a). 
 f'(c)*(g(b) - g(a)) - g'(c)*(f(b) - f(a)) = 0 , 
 \frac{f'(c)}{g'(c)} = \frac{f(b) - f(a)}{g(b) - g(a)}. 
f'(x),g'(x)
