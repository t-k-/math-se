[0,1]
a_n={(n+1)(n+2)\cdots 2n\over n^n}.
 \lim_{n\rightarrow\infty} {a_{n+1}\over a_n}=  \lim_{n\rightarrow\infty}{(2n+1)(2n+2)\over n+1}\cdot {n^n\over (n+1)^{n+1}}=  \lim_{n\rightarrow\infty}{2(2n+1)\over n+1}(1+\textstyle{1\over n})^{-n}={4\over e}. 
\lim\limits_{n\rightarrow\infty} \root n\of {a_n} = \lim\limits_{n\rightarrow\infty}{1\over n}\root n \of {(n+1)(n+2)\cdots 2n }. 
