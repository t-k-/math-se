(x_a, y_a), (x_b, y_b) 
(x_c, y_c)
 (-1, 2) = ( \frac{x_a + x_b} { 2}, \frac{ y_a + y_b } { 2} ), (5,0) = ( \frac{x_b + x_c} { 2}, \frac{ y_b + y_c } { 2} )
(7,4) = ( \frac{x_c + x_a} { 2}, \frac{ y_c + y_a } { 2} )
x_a = -1 + 7 -5 = 1, x_b = 5 +(-1) - 7 = -3, x_c = 7+5 - (-1) = 13
y_a = 2 + 4 - 0 = 6, y_b = 0 +2 - 4 = -2, y_c = 4 + 0 - 2 = 2 
