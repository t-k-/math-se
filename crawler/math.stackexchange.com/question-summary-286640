y'= \left( \begin{array}{cc} 13 & 12 \\ 12 & 13 \end{array} \right)  y+ \left(\begin{array}{c} x\\ 0 \end{array} \right)
y'=Ay+b(x)
y'=Ay
y_0
x_0=0
e^{A(x-x_0)}
A
y_h=e^{A(x-x_0)}y_0
(\mathbf{A} - \lambda \mathbf{I}) = 0
 \mathbf{A} = \begin{bmatrix}  13 & 12\\   12 & 13  \end{bmatrix} 
(\mathbf{A} - \lambda \mathbf{I}) = 0
 (\mathbf{A} - \lambda \mathbf{I}) = \begin{bmatrix}  13- \lambda & 12\\   12 & 13 - \lambda \end{bmatrix} = 0
(13 - \lambda)^{2} - 144 = 0, \text{so}, \lambda_{1,2} = 1, 25
(\mathbf{A} - \lambda_i \mathbf{I})\mathbf{x} = 0
\lambda_1 = 1, v1 = (1, 1)
\lambda_2 = 25, v2 = (-1, 1)
x_0=0
