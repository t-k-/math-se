X_1, X_2, ..., X_n
Y_1, Y_2, ..., Y_n
\mu_1
\mu_2
\sigma_1^2
\sigma_2^2
U_n = \frac{(\bar{X} - \bar{Y}) - (\mu_1 - \mu_2)}{\sqrt{\frac{(\sigma_1^2+\sigma_2^2)}{n}}                   }
U_n
n -> \infty
W_i = X_i - Y_i
i = 1, 2, ..., n
W_i=X_i-Y_i
\mu_W=E[W_i]=E[X_i-Y_i]=E[X_i]-E[Y_i]=\mu_1-\mu_2
W_i
\sigma^2_W=\sigma^2_1+\sigma^2_2
W_i
\sigma^2_1+\sigma^2_2
U_n=\frac{\frac{1}{n}\sum_{i=1}^nW_i-\mu_W}{\sqrt{\frac{\sigma^2_W}{n}}}
\sigma_W^2
\mu_W
\frac{1}{n}\sum_iW_i = \frac{1}{n}\sum_i (X_i - Y_i)=\frac{1}{n}\sum_i X_i-\frac{1}{n}\sum_i Y_i=\bar{X} - \bar{Y}
U_n=\frac{(\bar{X} - \bar{Y})-(\mu_1-\mu_2)}{\sqrt{\frac{\sigma^2_1+\sigma^2_2}{n}}}
