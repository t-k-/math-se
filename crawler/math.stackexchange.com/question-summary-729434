f(n)=6f(n-1)-9f(n-2)
n>1
f(0)=-1. f(1)=0
F(X)
F(X)=-1/(1-3x)^2
f(n)
 \frac 1{(1-x)^2} = \sum (n+1)x^n;\\ -\frac 1{(1-3x)^2} = -\sum (n+1)3^n x^n. 
 r^2 = 6r - 9\iff r=3. 
 f(n) = (A + Bn)3^n 
 -1 = A;\\ 0 = (A+B)\times 3;\\ f(n) = (n-1)3^n. 
F(x) = \sum_{n \ge 0} f(n) x^n
f(0) = -1
f(1) = 0
\begin{align} F(x)  &= f(0) + f(1)x + \sum_{n \ge 2} f(n) x^n \\ &= f(0) + f(1)x + \sum_{n \ge 2} (6f(n-1) - 9f(n-2))x^n \\ &= f(0) + f(1)x + 6x\sum_{n \ge 2} f(n-1)x^{n-1} - 9x^2\sum_{n \ge 2}f(n-2)x^{n-2}\\ &= f(0) + f(1)x + 6x\sum_{n \ge 1}f(n)x^n - 9x^2\sum_{n \ge 0}f(n)x^n \\ &= f(0) + f(1)x + 6x(F(x) - f(0)) - 9x^2F(x) \end{align}
F(x) = -1 + 6x(F(x) + 1) - 9x^2F(x)
(1 - 6x + 9x^2)F(x) = -1 + 6x
F(x) = \frac{-1 + 6x}{1- 6x + 9x^2} = \frac{-1+6x}{(1-3x)^2} = \frac{-1}{(1-3x)^2} + \frac{6x}{(1-3x)^2}
f(n) = -(n+1)3^n + 6n3^{n-1} = 3^n (-n - 1 + 2n) = 3^n(n - 1)
z^n
n \ge 0
F(z)
 f(n + 2) = 6 f(n + 1) - 9 f(n) \qquad f(0) = - 1, f(1) = 0 
 \frac{F(z) + 1}{z^2} = 6 \frac{F(z) + 1}{z} - 9 F(z) 
 F(z) = \frac{1 - 6 z}{1 - 6 z + 9 z^2} = \frac{1}{(1 - 3 z)^2} - \frac{2}{1 - 3 z} 
 f(n) = \binom{-2}{n} (-1)^n 3^n - 2 \cdot 3^n      = \left( \binom{n + 1}{1} - 2 \right) \cdot 3^n      = \frac{n - 3}{2} \cdot 3^n 
