 \begin{bmatrix} 0 & 0 & 0 & 0 & -3\\ 0 & 0 & 0 & -4 & 0\\ 0 & 0 & -1 & 0 & 0\\ 0 & 2 & 0 & 0 & 0\\ 5 & 0 & 0 & 0 & 0\\ \end{bmatrix} 
A = \begin{bmatrix} A_{11} & A_{12}\\ A_{21} & A_{22}\\ \end{bmatrix} 
 A_{11}= \begin{bmatrix} 0 & 0 & 0\\ 0 & 0 & 0\\ \end{bmatrix} 
 A_{12}= \begin{bmatrix} 0 & -3\\ -4 & 0\\ \end{bmatrix} 
 A_{21}= \begin{bmatrix} 0 & 0 & -1\\ 0 & 2 & 0\\ 5 & 0 & 0\\ \end{bmatrix} 
 A_{22}= \begin{bmatrix} 0 & 0\\ 0 & 0\\ 0 & 0\\ \end{bmatrix} 
