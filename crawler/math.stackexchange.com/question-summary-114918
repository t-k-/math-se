\begin{eqnarray*} \frac{2}{3}\int \frac{1}{\left( \frac{u}{\sqrt{3}/2}\right) ^{2}+1}\,du &=&% \frac{2}{3}\int \frac{1}{v^{2}+1}\cdot \frac{\sqrt{3}}{2}\,dv \\ &=&\frac{\sqrt{3}}{3}\int \frac{1}{v^{2}+1}\,dv \\ &=&\frac{\sqrt{3}}{3}\arctan v \\ &=&\frac{\sqrt{3}}{3}\arctan \left( \frac{u}{\sqrt{3}/2}\right)  \\ &=&\frac{\sqrt{3}}{3}\arctan \left( \frac{x+1/2}{\sqrt{3}/2}\right)  \\ &=&\frac{\sqrt{3}}{3}\arctan \left( \frac{\sqrt{3}}{3}\left( 2x+1\right) \right) +\text{Const.} \end{eqnarray*}
