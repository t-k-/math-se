 \begin{align} \left\|\int f(\cdot,y)\;\mathrm{d}y\right\|_p&=\sup_{\|h\|_{L^q}=1}\int\int h(x)f(x,y)\;\mathrm{d}y\;\mathrm{d}x\tag{1}\\ \int\|f(.,y)\|_p\;\mathrm{d}y&=\int\sup_{\|h\|_{L^q}=1}\int h(x)f(x,y)\;\mathrm{d}x\;\mathrm{d}y\tag{2} \end{align} 
 \begin{align} \left\|\int f(\cdot,y)\;\mathrm{d}y\right\|_p&=\sup_{\|h\|_{L^q}=1}\int\int h(x)f(x,y)\;\mathrm{d}y\;\mathrm{d}x\\ &\le\int\sup_{\|h\|_{L^q}=1}\int h(x)f(x,y)\;\mathrm{d}x\;\mathrm{d}y\\ &=\int\|f(.,y)\|_p\;\mathrm{d}y \end{align} 
