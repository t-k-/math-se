\newcommand{\bd}{\operatorname{bd}}
\bd(\bd(\bd(W)))=\bd(\bd(W))
W
(X,\mathscr{T})
A
\DeclareMathOperator{\bd}{bd}
\DeclareMathOperator{\cl}{cl} \DeclareMathOperator{\Int}{in} \DeclareMathOperator{\bd}{bd}
\cl
\Int
\bd(W)=\cl(W)-\Int(W)
\eqalign{\bd(\bd(W))&=\cl(\cl(W))-\Int(\Int(W))-\Int(\cl(W)-\Int(W))\cr&= \cl(W)-\Int(W)-\Int(\cl(W)-\Int(W)),}
\eqalign{\bd(\bd(\bd(W)))&=\cl(\cl(W)-\Int(W)-\Int(\cl(W)-\Int(W)))\cr&-\Int(\cl(W)-\Int(W)-\Int(\cl(W)-\Int(W)))\cr &=\cl(W)-\Int(W)-\Int(\cl(W)-\Int(W))\cr&-(\Int(\cl(W)-\Int(W))+\Int(\cl(W)-\Int(W))\cr &=\cl(W)-\Int(W)-\Int(\cl(W)-\Int(W))\cr &=\bd(\bd(W)).}
