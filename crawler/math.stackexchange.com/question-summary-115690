f'(x) = \frac{f(x+\epsilon) - f(x-\epsilon)}{2\epsilon}
\epsilon = 0.0001
f''(x) = \frac{f'(x+\epsilon) - f'(x-\epsilon)}{2\epsilon}
f''(x) = \frac{\frac{f(x+\epsilon+\epsilon) - f(x-\epsilon+\epsilon)}{2\epsilon} - \frac{f(x+\epsilon-\epsilon) - f(x-\epsilon-\epsilon)}{2\epsilon}}{2\epsilon}
