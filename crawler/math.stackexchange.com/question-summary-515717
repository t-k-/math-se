f(x)=1
f(x)=x^2
f(f(1))=f(p)=q=a_0+a_1p+a_2p^2+...+a_np^n\\p=a_0+a_1+a_2+..a_n
\bullet
a_0=a_1=...a_n=1\\q=1+p+p^2+...+p^n=\frac{1-p^{n+1}}{1-p},p=n+1\\q=\dfrac{(n+1)^{n+1}-1}{n}\implies f(x)=1+x+x^2+...+x^n
\bullet
a_0=a_1=...a_n=a\\q=a+ap+ap^2+...+ap^n=a\frac{1-p^{n+1}}{1-p},p=a+1\\q=\dfrac{(a+1)^{n+1}-1}{a}\implies f(x)=a+ax+ax^2+...+ax^n,a\ge0
