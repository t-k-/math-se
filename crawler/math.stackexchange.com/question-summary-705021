\begin{align} \sin(\delta -\phi ) &= \cos(\delta)\sin(-\phi)+\sin(\delta)\cos(-\phi) \\ &= -\cos(\delta)\sin(\phi)+\sin(\delta)\cos(\phi)\ \ \blacksquare \end{align}  
e^{i(\theta-\alpha)}
e^{i\theta - i\alpha} = e^{i\theta}\cdot e^{-i\alpha}\ .
e^{i(\theta - \alpha)} = e^{i\theta}\cdot e^{-i\alpha}\ 
\begin{align} e^{i\theta}\cdot e^{-i\alpha} &= (\cos(\theta)+i\sin(\theta))\cdot(\cos(-\alpha)+i\sin(-\alpha))\\ &= \cos(\theta)\cos(-\alpha)-\sin(\theta)\sin(-\alpha) + i(\cos(\theta)\sin(-\alpha)+\sin(\theta)\cos(-\alpha))\ \\  \end{align}
\begin{align} e^{i(\theta - \alpha)} = \cos(\theta - \alpha) + i \sin(\theta - \alpha) \end{align}\ .
\cos(\theta)\cos(-\alpha)-\sin(\theta)\sin(-\alpha) + i(\cos(\theta)\sin(-\alpha)+\sin(\theta)\cos(-\alpha))\ =  \cos(\theta - \alpha) + i \sin(\theta - \alpha)
\sin(-\theta) = -\sin(\theta)
\cos(-\theta) = \cos(\theta)
\begin{align}   \cos(\theta)\cos(-\alpha)-\sin(\theta)\sin(-\alpha) &= \cos(\theta + \alpha) &\\ \cos(\theta)\cos(\alpha)+\sin(\theta)\sin(\alpha)&= &\\ \\ \cos(\theta)\sin(-\alpha)+\sin(\theta)\cos(-\alpha) &=  \sin(\theta + \alpha) &\\ -\cos(\theta)\sin(\alpha)+\sin(\theta)\cos(\alpha) &=\ &\blacksquare \end{align} 
e^{x+y} = e^x e^y
e^{i(a+b)} = e^{ia}e^{ib}
e^{ix} = \cos{x} + i\sin{x}
\sin{x} = \frac{e^{ix} - e^{-ix}}{2i}
\cos{x} = \frac{e^{ix} + e^{-ix}}{2}
\sin{a}\cos{b} - \sin{b}\cos{a} = \frac{(e^{ia} - e^{-ia})(e^{ib} + e^{-ib})}{4i} - \frac{(e^{ib} - e^{-ib})(e^{ia} + e^{-ia})}{4i}\\ = \frac{e^{i(a+b)} - e^{i(-a+b)} + e^{i(a-b)} - e^{i(-a-b)} - e^{i(a+b)} + e^{i(a-b)} - e^{i(-a+b)} + e^{i(-a-b)}}{4i}
\sin{a}\cos{b} - \sin{b}\cos{a} =\frac{e^{i(a-b)} + e^{-i(a-b)}}{2i}
\sin{x} = \frac{e^{ix} - e^{-ix}}{2i}
\sin{a}\cos{b} - \sin{b}\cos{a} = \sin{(a-b)}
