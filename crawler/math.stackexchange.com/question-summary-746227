\sigma^2
\frac{\sigma_1^2}{\sigma_2^2}
 P[a < \frac{s_1^2/\sigma_1^2}{s_2^2/\sigma_2^2} < b] 
  P[a < \frac{s_1^2\sigma_2^2}{s_2^2\sigma_1^2} < b]
 P[\frac{s_2^2}{s_1^2}a < \frac{\sigma_2^2}{\sigma_1^2} < \frac{s_2^2}{s_1^2}b]
 P[\frac{s_1^2}{s_2^2b} < \frac{\sigma_1^2}{\sigma_2^2} < \frac{s_1^2}{s_2^2a}]
