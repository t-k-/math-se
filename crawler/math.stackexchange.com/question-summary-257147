\dim (L) = 3
\dim (L') = 2
L'
\operatorname{ad} x : L' \rightarrow L'
\operatorname{ad} x : L' \rightarrow L'
\operatorname{ad} x : L' \rightarrow L
\operatorname{ad} x([y,z]) = [\operatorname{ad} x(y), \operatorname{ad}x(z)]
y,z \in L'
L'
\rm{ad}_x([y,z]) = \rm{ad}_x(0) = 0
\rm{ad}_x(y) = [x,y]
\rm{ad}_x(z) = [x,z]
L'
[\rm{ad}_x(y),\rm{ad}_x(z)] = 0
