\newcommand{\ra}[1]{\kern-1.5ex\xrightarrow{\ \ #1\ \ }\phantom{}\kern-1.5ex} \newcommand{\ras}[1]{\kern-1.5ex\xrightarrow{\ \ \smash{#1}\ \ }\phantom{}\kern-1.5ex} \newcommand{\da}[1]{\bigg\downarrow\raise.5ex\rlap{\scriptstyle#1}} \begin{array}{c}    I\times I\setminus\{p\}    & \ra{r}       &    \partial (I\times I)     \\   \da{p}     &              &  \da{p|_{\partial(I\times I)}}               \\    (I\times I\setminus\{p\})/{\sim}       & \ras{\tilde{r}} &    \partial (I\times I)/{\sim}                        \\ \end{array} 
