z = -5 + 4i
z = 7 + 2i
(-5 + 4i) a + (-5 - 4i) b =38
(7 + 2i) a + (7 - 2i) b = 38
(12 - 2i) a + (12 + 2i) b = 0
 b = -\frac{6 - i}{6 + i} a.
(-5 + 4i) a - (-5 - 4i) \cdot \frac{6 - i}{6 + i} a = 38.
a = 1 - 6i
b = 1 + 6i
 ab = (1 - 6i)(1 + 6i) = \boxed{37}. 
