\textit{Natural Deduction}
\textit{Logic and Structure}
\to
(P_{1} \lor \neg P_{2})
\neg(P_{1} \to P_{2}) \leftrightarrow(P_{1} \lor \neg P_{2})
\neg(P \to Q) \to (P \lor \neg Q)
\quad\neg(P \to Q)
\quad\quad\ldots
\quad(P \lor \neg Q)
\_\_\_\_\_\_\_\_\_\_\_\_
\neg(P \to Q) \to (P \lor \neg Q)
(P \lor \neg Q)
\lor
\quad\neg(P \to Q)
\quad\quad\ldots
\quad\quad \neg Q
\quad(P \lor \neg Q)
\_\_\_\_\_\_\_\_\_\_\_\_
\neg(P \to Q) \to (P \lor \neg Q)
\neg Q
