 \begin{eqnarray*} \sin \frac{\pi }{8} &=&\sin \frac{7\pi }{8}=\cos \frac{3\pi }{8}=\frac{1}{2} \sqrt{2-\sqrt{2}}, \\ && \\ \sin \frac{3\pi }{8} &=&\sin \frac{5\pi }{8}=\cos \frac{\pi }{8}=\frac{1}{2} \sqrt{2+\sqrt{2}}, \\ && \\ \cos \frac{5\pi }{8} &=&\cos \frac{7\pi }{8}=-\frac{1}{2}\sqrt{2-\sqrt{2}}. \end{eqnarray*} 
