\dfrac{a^3}{b^2-bc+c^2}+\dfrac{b^3}{a^2-ac+c^2}+\dfrac{c^3}{a^2-ab+b^2}\geq a+b+c
\dfrac{a^3}{b^2-bc+c^2}+\dfrac{b^3}{a^2-ac+c^2}+\dfrac{c^3}{a^2-ab+b^2}\geq a+b+c \iff \dfrac{a^3(b+c)}{b^3+c^3}+\dfrac{b^3(a+c)}{a^3+c^3}+\dfrac{c^3(b+a)}{b^3+a^3} \geq a+b+c \iff a^9b+a^9c+b^9a+b^9c+c^9a+c^9b \ge a^7b^3+a^7c^3+b^7a^3+b^7c^3+c^7a^3+c^7b^3
a^n+b^n\ge a^{n-1}b+b^{n-1}a \ge a^{n-2}b^2+b^{n-2}a^2 \ge ...
a^n+b^n\ge a^{n-1}b+b^{n-1}a \iff (a-b)(a^{n-1}-b^{n-1})\ge 0 \iff (a-b)^2(a^{n-2}+a^{n-3}b+a^{n-4}b^2+....ab^{n-3}+b^{n-2}) \ge 0
a=b
a^9b+b^9a \ge a^8b^2+a^8b^2 \ge a^7b^3+b^7a^3
a=\min{\{a,b,c\}}
\Longleftrightarrow \dfrac{a^4}{ab^2-abc+ac^2}+\dfrac{b^4}{bc^2-abc+ba^2}+\dfrac{c^4}{ca^2-abc+cb^2}\ge a+b+c
\left[ \dfrac{a^4}{ab^2-abc+ac^2}+\dfrac{b^4}{bc^2-abc+ba^2}+\dfrac{c^4}{ca^2-abc+cb^2}\right][bc(b+c)+ac(a+c)+ab(a+b)-3abc]\ge (a^2+b^2+c^2)^2
\Longleftrightarrow (a^2+b^2+c^2)^2\ge (a+b+c)[(bc(b+c)+ac(a+c)+ab(a+b)-3abc]
\Longleftrightarrow a^2(a-b)(a-c)+(b^2+c^2+bc-ab-ac)(b-c)^2\ge 0
