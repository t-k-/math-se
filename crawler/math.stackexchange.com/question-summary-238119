\newcommand{\sgn}{\operatorname{sgn}}f: (x,y) \mapsto \begin{cases} \frac{(x \sin y)^2}{|x|+|y|},&(x,y) \neq (0,0) \\                                  0, & (x,y) = (0,0)   \end{cases}
\frac{\partial f(x,y)}{\partial x} =  \begin{cases} \sin^2 y\frac{2x(|x|+|y|)-x^2\sgn(x)}{(|x|+|y|)^2}, & (x,y)\neq (0,0)\\   0, & (x,y) = (0,0)\end{cases}  
\frac{\partial f(x,y)}{\partial y} = \begin{cases} x^2\frac{\sin^2y\cdot \cos y(|x|+|y|)-\sin^2(y)\sgn(y)}{(|x|+|y|)^2}, &(x,y) \neq (0,0) \\ 0, &(x,y) = (0,0)\end{cases}
(x,y) \neq (0,0)
