W=0
X=0 \cap Y \geq1
W=1 \ X=1 \ \text{and} \ Y \geq 2
W
 \mathbf{P}(W \leq k)= \sum_{j=0}^{k}p_j \mathbf{P}(Y \geq j+1) + \sum_{j=0}^{k}p_j \mathbf{P}(X \geq j+1) 
 F_{W}(w)=\mathbf{P}(W \leq w)=\int_{0}^{w}f_{X}(w')\mathbf{P}(Y \geq w')dw' + \int_{0}^{w}f_{Y}(w')\mathbf{P}(X \geq w')dw'\\ = 2 \int_{0}^{w}(1-w')dw'=2w \bigg( 1-\frac{w}{2}\bigg) 
 f(w)=2(1-w) 
 \mathbf{E}W = \int_{0}^{1}w f(w) dw=2 \int_{0}^{1}w(1-w)dw=\frac{1}{3} 
