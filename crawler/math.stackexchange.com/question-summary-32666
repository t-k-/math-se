s_0[n] = 1, 1, 1, 1, ...
s_1[n] = \sum_{i=1}^{n}s_0[i]=1,2,3,4,...
s_2[n] = \sum_{i=1}^{n}s_1[i]=1,3,6,10,...
s_k[n] = \sum_{i=1}^{n}s_{k-1}[i]
s_k[n]
s_k[n] = {n+k \choose n}
f_n
F(x) = \sum_{n \ge 0} f_n x^n
\displaystyle s_n = \sum_{k=0}^n f_k
\sum_{n \ge 0} s_n x^n = \frac{F(x)}{1 - x}.
s_k[n]={{n+k}\choose k}
n=0
