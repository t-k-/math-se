f(x,y) = \begin{cases} \displaystyle \frac{xy(x^2-y^2)}{x^2+y^2} & \text{if } (x,y) \neq (0,0), \\ 0 & \text{if } (x,y) = (0,0). \end{cases}
f_y(0,0)=0
(0,0)
\nabla f(0,0)=\binom 00
   f_x(0,0) ~=~   \lim_{h\to 0}   \frac{f(h,0)-f(0,0)} h ~=~   \lim_{h\to 0}   \frac{\frac{h\cdot 0(h^2-0^2)}{h^2+0}-0} h ~=~   \lim_{h\to 0}   \frac{0}{h} ~=~   0 
f_x(0,0)
f_y(0,0)
f_{xy}(0,0)
f_{yx}(0,0)
(x,y)\neq(0,0)
h,k\neq 0
 f_{xy}(0,0) ~=~ \lim_{k\to 0} \frac{f_x(0,k)-f_x(0,0)}k ~=~ \lim_{k\to 0} \frac{f_x(0,k)}k 
 f_{yx}(0,0) ~=~ \lim_{h\to 0} \frac{f_y(h,0)-f_y(0,0)}h ~=~ \lim_{h\to 0} \frac{f_y(h,0)}h 
 f_x(x,y)=\frac{y\big(x^4+4x^2y^2-y^4\big)}{\big(x^2+y^2\big)^2} 
 f_y(x,y)=\frac{x\big(x^4-2x^2y^2-y^4\big)}{\big(x^2+y^2\big)^2} 
 f_x(0,k)=-k \quad\text{and}\quad f_y(h,0)=h 
 f_{xy}(0,0) ~=~ \lim_{k\to 0} \frac{f_x(0,k)}k ~=~ \lim{k\to 0}\frac{-k}{k} ~=~ -1 
 f_{yx}(0,0) ~=~ \lim_{h\to 0} \frac{f_y(h,0)-f_y(0,0)}h ~=~ \lim_{h\to 0} \frac{f_y(h,0)}h ~=~ \lim{h\to 0}\frac{h}{h} ~=~ 1 
 f_{xy}(0,0)~=~-1 ~~\neq~~ 1~=~f_{yx}(0,0) 
