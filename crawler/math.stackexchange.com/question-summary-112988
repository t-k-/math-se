\begin{align} \Gamma = \int_C \vec V(x,y) \cdot d\vec r &= \\ &= \int_{C_1} \vec V(x,y) \cdot d\vec r + \int_{C_2} \vec V(x,y) \cdot d\vec r - \int_{-C_3} \vec V(x,y) \cdot d\vec r - \int_{-C_4} \vec V(x,y) \cdot d\vec r \\ &= \int^{b_1}_{a_1} \vec V(\vec r_1(t)) \cdot r_1'(t) dt + \int^{b_2}_{a_2} \vec V(\vec r_2(t)) \cdot r_2'(t) dt \\ &- \int^{b_3}_{a_3} \vec V(\vec r_3(t)) \cdot r_3'(t) dt -\int^{b_4}_{a_4} \vec V(\vec r_4(t)) \cdot r_4'(t) dt \end{align}
