\begin{align*} (A'\cup B)\cap(A\cup B') &= (A'\cap(A\cup B'))\cup (B\cap(A\cup B'))\\ &= ((A'\cap A)\cup(A'\cap B'))\cup ((B\cap A)\cup(B\cap B'))\\ &= (\varnothing \cup (A'\cap B'))\cup ((B\cap A)\cup \varnothing))\\ &= (A'\cap B')\cup(B\cap A) \end{align*}
