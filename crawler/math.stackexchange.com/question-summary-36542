f(z) = \frac{az+b}{cz+d}
ad-bc \neq 0
a,b,c
d
a= a_1 + a_2i
\frac{(a_1c_1+a_2c_2)z^2 + (b_1c_1+b_2c_2)z +(a_1d_1+a_2d_2)z +(b_1d_1+b_2d_2)}{|c|^2z^2 + |d|^2}
\frac{(a_1c_1-a_2c_2)z^2 + (b_1c_1-b_2c_2)z +(a_1d_1-a_2d_2)z +(b_1d_1-b_2d_2)}{|c|^2z^2 + |d|^2}i
\frac{(a_1x-a_2y+b_1)(c_1x-c_2y+d_1) -(a_2x+a_1y+b_2)(c_2x+c_1y+d_2)}{(c_1x-c_2y+d_1)^2 - (c_2x+c_1y+d_2)^2}
\frac{R(ac)x^2 - 2(a_1c_2 + a_2c_1)xy + (R(ad) + R(bc))x - (R(ab) + R(bc))y - R(ac)y^2 + R(bd)}{(c_1x-c_2y+d_1)^2 - (c_2x+c_1y+d_2)^2}
\frac{(a_2x-a_1y+b_2)(c_1x-c_2y+d_1) -(a_1x+a_2y+b_1)(c_2x+c_1y+d_2)}{(c_1x-c_2y+d_1)^2 - (c_2x+c_1y+d_2)^2}
z=x+iy
\Re\left(\frac{az+b}{cz+d}\right)=\frac{\mu\,\Re(a)+\nu\,\Im(a)+\xi\,\Re(b)+\eta\,\Im(b)}{\Delta}
\Im\left(\frac{az+b}{cz+d}\right)=\frac{\mu\,\Im(a)-\nu\,\Re(a)+\xi\,\Im(b)-\eta\,\Re(b)}{\Delta}
\mu=|z|^2\Re(c)+\alpha
\nu=|z|^2\Im(c)+\beta
\xi=x\Re(c)-y\Im(c)+\Re(d)
\eta=x\Im(c)+y\Re(c)+\Im(d)
\Delta=|z|^2 |c|^2+|d|^2+2\alpha\,\Re(c)+2\beta\,\Im(c)
\alpha=x\Re(d)+y\Im(d)
\beta=x\Im(d)-y\Re(d)
z
z=x+iy
f(z)=u(x,y)+iv(x,y)
