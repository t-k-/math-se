\sin\theta
\cos\theta
t\leftarrow\frac{2\theta}\pi
s\leftarrow\sin\theta
c\leftarrow\cos\theta
s^2+c^2=1
r= \lfloor t \rfloor
r\equiv 0\pmod 4
s\ge 0, c\ge 0
r\equiv 1\pmod 4
s\ge 0, c\le 0
r\equiv 2\pmod 4
s\le 0, c\le 0
r\equiv 3\pmod 4
s\le 0,c\le 0
t\leftarrow 2t
s\leftarrow 2sc
c\leftarrow 2c^2-1
