n=0,1,2,...
u=x^{4/3}
 \int_{0}^{\infty}  e^{-u}u^{1/2}(cos u)  du, 
\Gamma(3/2)=\frac{\sqrt{\pi}}{2}
\lambda=1
\nu=3/2
(1-i)^{-3/2}
\frac{4}{3}x^{1/3}
t=x^{4/3}
z=(1\pm \mathrm i)t
 \begin{eqnarray} \int_{0}^{\infty} e^{-x^{4/3}} \cos x^{4/3} x^{4n+1}\;\mathrm dx &=& \int_{0}^{\infty} e^{-x^{4/3}} \left(\frac{e^{\mathrm ix^{4/3}}+e^{-\mathrm ix^{4/3}}}{2}\right) x^{4n+1}\;\mathrm dx=\\ &=&\frac{1}{2}\int_{0}^{\infty} e^{-x^{4/3}(1+\mathrm i)}  x^{4n+1}\;\mathrm dx+\frac{1}{2}\int_{0}^{\infty} e^{-x^{4/3}(1+\mathrm i)}  x^{4n+1}\;\mathrm dx=\\ \end{eqnarray} 
 \begin{eqnarray} \int_{0}^{\infty} e^{-x^{4/3}(1\pm\mathrm i)}  x^{4n+1}\;\mathrm dx&=&\int_{0}^{\infty} e^{-x^{4/3}(1\pm\mathrm i)}  x^{4n+1}\left(\frac{3}{4}x^{-1/3}\right)\;\mathrm dx^{4/3}=\\ &=&\frac{3}{4}\int_{0}^{\infty} e^{-t(1\pm\mathrm i)}  t^{3n+\frac{1}{2}}\;\mathrm dt \end{eqnarray} 
 \begin{eqnarray} \frac{3}{4}\int_{0}^{\infty} e^{-t(1\pm\mathrm i)}  t^{3n+\frac{1}{2}}\;\mathrm dt&=& \frac{3}{4}(1\pm\mathrm i)^{-3n-\frac{3}{2}}\int_{0}^{\infty} e^{-z}  z^{3n+\frac{1}{2}}\;\mathrm dt=\\ &=&\frac{3}{4}(1\pm\mathrm i)^{-3n-\frac{3}{2}}\Gamma\left(3n+\frac{3}{2}\right) \end{eqnarray} 
 \begin{eqnarray} \int_{0}^{\infty} e^{-x^{4/3}} \cos x^{4/3} x^{4n+1}\;\mathrm dx &=&  \frac{3}{8}\Gamma\left(3n+\frac{3}{2}\right)\left((1+\mathrm i)^{-3n-\frac{3}{2}}+(1-\mathrm i)^{-3n-\frac{3}{2}}\right)=\\ &=&\frac{3}{8}\Gamma\left(3n+\frac{3}{2}\right)2^{-\frac{3n}{2}-\frac{3}{4}}\left(e^{\mathrm i \frac{\pi}{4}(-3n-\frac{3}{2})}+e^{-\mathrm i \frac{\pi}{4}(-3n-\frac{3}{2})}\right)=\\ &=&\frac{3}{8}\Gamma\left(3n+\frac{3}{2}\right)2^{-\frac{3n}{2}+\frac{1}{4}}\cos\left(\frac{3\pi}{4}\left(n+\frac{1}{2}\right)\right) \end{eqnarray} 
