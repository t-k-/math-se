\begin{align} \dfrac{\displaystyle{83 - L}\choose{\displaystyle L}}{\displaystyle{82}\choose{\displaystyle L}} \end{align}
\begin{align} \frac{\color{red}{{83}\choose{L}}}{\color{green}{{82}\choose{L}}}=\color{red}{\frac{83!}{L!(83-L)!}}\cdot\color{green}{\frac{L!(82-L)!}{82!}}=\frac{83}{83-L} \end{align}
\begin{aligned} \dfrac{\color{red}{\dbinom{83}{L}}}{\color{blue}{\dbinom{82}{L}}} &= \dfrac{\color{red}{\dfrac{83!}{L!(83 - L)!}}}{\color{blue}{\dfrac{82!}{L!(82 - L)!}}} & \text{[Definition of binomial coefficients]}\\ &= \dfrac{\color{red}{83!}\color{blue}{(82 - L)!}}{\color{blue}{82!}\color{red}{(83 - L)!}} & \text{With some algebra..}\\ &= \dfrac{\color{red}{83 \cdot 82!}\color{blue}{(82 - L)!}}{\color{blue}{82!}\color{red}{(83 - L) \cdot (82 - L)!}} & \text{...by dividing terms...}\\ &= \color{green}{\dfrac{83}{83 - L}} & \text{...we get this expression.} \end{aligned}
{a\choose b}
\frac{{83}\choose{L}}{{82}\choose{L}} = \frac{83!}{L!\cdot\left(83-L\right)!}\cdot\frac{L!\cdot\left(82-L\right)!}{82!} = \frac{83}{83-L}
