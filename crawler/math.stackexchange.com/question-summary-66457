\begin{align*} \frac{d}{dt}\arcsin\left(\frac{2t}{1+t^2}\right) &= \frac{1}{\sqrt{1 - \frac{4t^2}{(1+t^2)^2}}}\left(\frac{2t}{1+t^2}\right)'\\ &= \frac{(1+t^2)}{\sqrt{(1+t^2)^2-4t^2}}\left(\frac{2(1+t^2)-4t^2}{(1+t^2)^2}\right)\\ &= \frac{(1+t^2)}{\sqrt{(1-t^2)^2}}\left(\frac{2(1-t^2)}{(1+t^2)^2}\right)\\ &= \frac{2(1-t^2)}{(1+t^2)\sqrt{(t^2-1)^2}} =\frac{2(1-t^2)}{(1+t^2)|t^2-1|}. \end{align*}
