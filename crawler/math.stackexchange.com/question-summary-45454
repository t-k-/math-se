x^2 + y^2 = 9
\frac{dy}{dx}= \frac{x}{y}.
y'
\frac{dy}{dx}
y'
\frac{dy}{dx} = \frac{y-x^2}{y^2-x}
x^3+y^3 = 3xy.
\begin{align*} x^3 + y^3 &= 3xy\\ \frac{d}{dx}\left( x^3+y^3\right) &= \frac{d}{dx}3xy\\ \frac{d}{dx}x^3 + \frac{d}{dx}y^3 &= \frac{d}{dx}3xy &&\text{(since }(f+g)'=f'+g'\text{)}\\ 3x^2 + \frac{d}{dx}y^3 &= \frac{d}{dx}3xy &&\text{(since }(x^3)'=3x^2\text{)}\\ 3x^2 + 3y^2\left(\frac{dy}{dx}\right) &= \frac{d}{dx}3xy &&\text{(using the Chain Rule)}\\ 3x^2 + 3y^2y' &= \frac{d}{dx}3xy &&\text{(we don't know what }\frac{dy}{dx}\text{ is, so leave indicated)}\\ 3x^2 + 3y^2y' &= 3\frac{d}{dx}(xy) &&\text{(since }(3f)' = 3f'\text{)}\\ 3x^2+3y^2y' &= 3\left( x'y + xy'\right) &&\text{(product rule)}\\ 3x^2 +3y^2y' &= 3(y+xy') &&\text{(since }x'=1\text{)}\\ 3x^2 + 3y^2y' &=3y + 3xy' \end{align*}
\begin{align*} 3x^2 + 3y^2y' &= 3y+3xy'\\ 3y^2y' - 3xy' &= 3y - 3x^2\\ 3(y^2-x)y' &= 3(y-x^2)\\ y' &= \frac{3(y-x^2)}{3(y^2-x)}\\ y' &= \frac{y-x^2}{y^2-x}. \end{align*}
