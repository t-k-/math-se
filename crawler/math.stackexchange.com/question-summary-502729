\begin{align} F & =AB'C + (A'B' + ABC'D)'\tag{1}\\ \\  & = AB'C + (A'B')' (ABC'D)'\tag{2}\\ \\  & = AB'C + (A + B) (A' + B' + C + D')\tag{3}\\ \\  &  = AB'C + (AA'+AB'+AC+AD') (BA'+BB'+BC+BD')\tag{4}\end{align}
\begin{align} F &= AB'C + (AA'+AB'+AC+AD') \color{red}{\bf +} (BA'+BB'+BC+BD')\tag{4}\\ \\ & = AB'C + AB' + AC + AD' + BA' + BC + BD'\tag{5}\\ \\ & = AB'(C + 1) + AC + AD' + BA' + BC + BD'\tag{6} \\ \\ & =  AB' + AC + AD' + BA' + BC + BD' \tag{7}\\ \\ & = AB' + BA' + (A+B)(C + D')\tag{8}\end{align}
