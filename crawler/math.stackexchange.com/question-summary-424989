<x,y>^2=<x^2,xy,y^2>.
<2,\sqrt{6}>^2=<4,2\sqrt{6},6>=<2,2\sqrt{6}>.
<x,y>=\{r_1x+r_2y |r_1,r_2 \in R \},
<x,y>^2=\{ a_1b_1+a_2b_2 | a_1,b_1,a_2,b_2 \in <x,y> \}
= \{ (r_{1,1}x+r_{1,2}y)(r_{2,1}x+r_{2,2}y)+(r_{3,1}x+r_{3,2}y)(r_{4,1}x+r_{4,2}y) | r_{i,j} \in R \}
=\{ r_{1,1}r_{2,1}x^2+r_{1,2}r_{2,1}xy+r_{1,1}r_{2,2}xy+r_{1,2}r_{2,2}y^2+r_{3,1}r_{4,1}x^2+r_{3,2}r_{4,1}xy+r_{3,1}r_{4,2}xy+r_{3,2}r_{4,2}y^2 | r_{i,j} \in R \}
=\{ (r_{1,1}r_{2,1}+r_{3,1}r_{4,1})x^2+(r_{1,2}r_{2,1}+r_{1,1}r_{2,2}+r_{3,2}r_{4,1}+r_{3,1}r_{4,2})xy+(r_{1,2}r_{2,2}+r_{3,2}r_{4,2})y^2 | r_{i,j} \in R \}
=\{ r_{1,1}x^2+(r_{1,2}+r_{3,1})xy+r_{3,2}y^2 | r_{i,j} \in R \}
=<x^2,xy,y^2>.
