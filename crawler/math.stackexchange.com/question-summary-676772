f : \mathbb R^2 \to \mathbb R
g :\mathbb R^2 \to\mathbb R
g(x, y) = f(f(x, y), x)
d(g(x, y))/dx
d(g(x, y))/dy
f
f(x, y) = x + y^2
 \newcommand{\pwrt}[2]{\frac{\partial #1}{\partial #2}} 
g(x,y) = f(f(x,y),x)
x_0(x,y) = f(x,y)
y_0(x,y) = x
x
f \to x_0 \to x
f \to y_0 \to x
f(x,y) = x + y^2
 g(x,y) = f(x_0(x,y),y_0(x,y)) 
 \begin{align} \pwrt{g}{x} &= \pwrt{g}{x_0}\pwrt{x_0}{x} + \pwrt{g}{y_0}\pwrt{y_0}{x}\\ &= \pwrt{f}{x}(x_0,y_0)\cdot \pwrt{x_0}{x}+ \pwrt{f}{y}(x_0,y_0)\cdot \pwrt{y_0}{x}\\ &= \pwrt{f}{x}(x_0,y_0)\cdot \pwrt{f}{x}(x,y)+ \pwrt{f}{y}(x_0,y_0)\cdot 1\\ &= \pwrt{f}{x}(f(x,y),x)\cdot \pwrt{f}{x}(x,y)+ \pwrt{f}{y}(f(x,y),x) \end{align} 
 \begin{align} &f(f(x,y),x) = (f(x,y)) + x^2 = (x^2 + y) + x^2 = 2x^2 + y\\ &\pwrt{f}{x}(x,y) = 1\\ &\pwrt{f}{y}(x,y) = 2y\\ &\pwrt{f}{x}(f(x,y),x) = 1\\ &\pwrt{f}{y}(f(x,y),x) = 2x\\ \end{align} 
D_{1}G(x,y) = D_{1}f_{(f(x,y),x)}\cdot D_{1}f_{(x,y)} + D_{2}f_{(f(x,y),x)}\cdot D_{1}x  = \frac{\partial f}{\partial x}(f(x,y),x)\cdot \frac{\partial f}{\partial x}(x,y) + \frac{\partial f}{\partial y}(f(x,y),x)
D_{2}G(x,y) = D_{1}f_{(f(x,y),x)}\cdot D_{2}f_{(x,y)} +
D_{2}f_{(f(x,y),x)}\cdot D_{2}x = \frac{\partial f}{\partial x}(f(x,y),x)\cdot \frac{\partial f}{\partial y}(x,y) 
f(x,y) = x + y^2
D_{1}G = 1 + 2x
D_{2}G = 2y
u = f(x, y)
z = f(u, x) = g(x, y)
 \frac{\partial z}{\partial x} = \frac{\partial z}{\partial u} \cdot \frac{\partial u}{\partial x} + \frac{\partial z}{\partial x} 
 \frac{\partial z}{\partial y} = \frac{\partial z}{\partial u} \cdot \frac{\partial u}{\partial y}
