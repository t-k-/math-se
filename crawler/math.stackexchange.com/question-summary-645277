 \frac{P0 \Rightarrow P1\ P0}{P1} 
 \frac{P0\ OR\ P1}{P0\ P1} 
 \frac{R}{\frac{P\ \vdash Q\ ,  Q\vdash\ R\ ,\ \ P\ AND\ Q\ \Rightarrow\ R}{(P\ \Rightarrow\ Q)\  OR\  (Q\ \Rightarrow\ R)\ \Rightarrow\ (P\ AND \ Q\ \Rightarrow\ R)}} 
 \frac{\frac{Q\ \vdash\ FALSE}{NOT(Q)},\ \frac{R\ \vdash\ FALSE}{NOT(R)}}{P\ \Rightarrow\ !Q\ AND\ !R} 
 \frac{Q,P\ \vdash\ NOT(Q)\ , NOT(P)}{(P\ \Rightarrow\ Q)\Rightarrow\ !Q\Rightarrow\ !P} 
