 \begin{equation} V = \left|\frac{\det( V_{ij} ,i,j)}{d!}\right| , \tag{1} \end{equation} 
 \begin{equation} V_{ij} = p_{ij} - p_{i,m} \tag{2} \end{equation} 
S_{kn}
S_n = \sqrt{\delta_{qn}S_{kq}S_{kn}}
s_{kn} = S_{kn}/S
 \begin{equation} S_{kn} = \frac{\det(S_{ijkn},i,j)}{(d-1)!}  \tag{3} \end{equation} 
 \begin{equation} S_{ijkn} = V_{ij}(1 - \delta_{j,n}) + \delta_{j,n}\delta_{i,k} \tag{4}. \end{equation} 
