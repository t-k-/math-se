\small\begin{align*} \Bigl(f_1\cdots f_nf_{n+1}\Bigr)' &= \Bigl( (f_1\cdots f_n)f_{n+1}\Bigr)'\\ &= (f_1\cdots f_n)'f_{n+1} + (f_1\cdots f_n)f_{n+1}' \\ &= \Bigl(f_1'f_2\cdots f_n + f_1f_2'f_3\cdots f_n + \cdots + f_1\cdots f_{n-1}f_n'\Bigr)f_{n+1} + (f_1\cdots f_n)f_{n+1}'\\ &= f_1'f_2\cdots f_nf_{n+1} + f_1f_2'f_3\cdots f_nf_{n+1} + \cdots + f_1\cdots f_n'f_{n+1} + f_1\cdots f_nf_{n+1}'. \end{align*}
