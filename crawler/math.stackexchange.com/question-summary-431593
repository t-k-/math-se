a^3+b^3-a^2b^2-(a+b)^2 c=0
a>0,b>0,
a^3+b^3-a^2b^2-(a+b)^2 c=0 
\Rightarrow a^3+b^3\equiv a^2b^2 \pmod {(a+b)^2}
\Rightarrow ab\equiv 0 \pmod {a+b}
\dfrac{ab}{a+b}=k\in \mathbb Z,(a,b)=d,a=a_1 d,b=b_1 d,(a_1,b_1)=1,\dfrac{a_1b_1d}{a_1+b_1}=k.
(a_1b_1,a_1+b_1)=1,a_1+b_1\mid d.
d=t(a_1+b_1),
k=a_1b_1t,a=a_1(a_1+b_1)t,b=b_1(a_1+b_1)t.
c=\dfrac{a^3+b^3-a^2b^2}{(a+b)^2}=(a_1^2-a_1b_1+b_1^2-ta_1^2b_1^2)t<(a_1^2+b_1^2-a_1^2b_1^2)t=(1-(a_1^2-1)(b_1^2-1))t
a_1>1
b_1>1,
c<0.
a_1=1
b_1=1,
\dfrac{c}{t}\in \mathbb Z,
\dfrac{c}{t}<1
\dfrac{c}{t}=0.
a>0
b>0
c=0.
a>0,b>0,c=0,
a^3+b^3-a^2b^2=0,(a_1^3+b_1^3)=da_1^2b_1^2,
(a_1^3+b_1^3,a_1^2b_1^2)=1,
a_1=b_1=1,d=2.
a=b=2,c=0.
a=0,
b^3-b^2c=0, b=0
c=b.
