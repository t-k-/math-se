 \ddot{\theta_1}=-L_1\sin\theta_1 + \frac{m_2}{m_1}\cos\theta_2\sin(\theta_2-\theta_1),\\ \ddot{\theta}_2=-L_2\sin\theta_2. 
 \dot{\theta}_1=w_1\\ \dot{\theta}_2=w_2\\ \dot{w}_1=-L_1\sin\theta_1+\frac{m_2}{m_1}\cos\theta_2\sin(\theta_2-\theta_1)\\ \dot{w}_2=-L_2\sin\theta_2. 
\frac12 m_1 (\dot{x_1}^2 + \dot{y_1}^2) + \frac12 m_2 (\dot{x_2}^2 + \dot{y_2}^2) 
(x_1,y_1)
x_1 = L_1 \sin{\theta_1}
y_1 = L_1 (1-\cos{\theta_1}) 
x_2 = x_1 + L_2 \sin{(\theta_2-\theta_1)} 
y_2 = y_1 + L_2 (1-\cos{(\theta_2-\theta_1)}) 
m_1 g y_1+ m_2 g y_2
