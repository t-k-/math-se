\begin{align*} 1&=a_0\,;\\ 0&=a_1-\frac{a_0}2=a_1-\frac12,\text{ so }a_1=\frac12\,;\\ 0&=a_2-\frac{a_1}2+\frac{a_0}3=a_2-\frac14+\frac13=a_2+\frac1{12},\text{ so }a_2=-\frac1{12}\,; \end{align*}
