a\in\mathbb{Z}
n = 2k+1
c_n= 0
n = 2k
k > a
2 \binom{2a}{2k}
 c_n = \binom{a}{0} \binom{a}{n} - \binom{a}{1} \binom{a}{n-1} + \cdots+ (-1)^n \binom{a}{n} \binom{a}{0} = \sum_{i=0}^n (-1)^i \binom{a}{i} \binom{a}{n-i}
 2 \sum_{i=0}^k \binom{a}{i} \binom{a}{2k-i}  +  (-1)^{k+1} \binom{a}{k+1} 
\{a_n\}
\{b_n\}
 A(z) B(z) = \sum_{n\ge 0} a_n \frac{z^n}{n!} \sum_{n\ge 0} b_n \frac{z^n}{n!} = \sum_{n\ge 0} \sum_{k=0}^n \frac{1}{k!}\frac{1}{(n-k)!} a_k b_{n-k} z^n\\ = \sum_{n\ge 0} \sum_{k=0}^n \frac{n!}{k!(n-k)!} a_k b_{n-k} \frac{z^n}{n!} = \sum_{n\ge 0} \left(\sum_{k=0}^n {n\choose k} a_k b_{n-k}\right)\frac{z^n}{n!}
\sum_{k=0}^n {n\choose k} a_k b_{n-k}.
Q_n = \sum_{k=0}^n (-1)^k {a\choose k}{a\choose n-k} = \frac{1}{n!}  \sum_{k=0}^n{n\choose k} \frac{ (-1)^k \times a!}{(a-k)!} \frac{a!}{(a-(n-k))!}
A(z) = \sum_{q=0}^a \frac{(-1)^q \times a!}{(a-q)!}\frac{z^q}{q!} = \sum_{q=0}^a {a\choose q} (-z)^q = (1-z)^a.
B(z) = \sum_{q=0}^a \frac{a!}{(a-q)!}\frac{z^q}{q!} = \sum_{q=0}^a {a\choose q} z^q =  (1+z)^a.
 \frac{1}{n!} \times n! [z^n] A(z) B(z) =  [z^n] (1-z)^a (1+z)^a = [z^n] (1-z^2)^a.
Q_n = \begin{cases} 0 \quad && \text{if} \quad n \quad \text{is odd} \\ (-1)^{n/2} {a\choose n/2} \quad && \text{if} \quad n \quad \text{is even.} \\ \end{cases}.
