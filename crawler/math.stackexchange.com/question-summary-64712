\begin{align*} \begin{bmatrix} A_{m \times m} & B_{m \times n}\\  C_{n \times m} & D_{n \times n} \end{bmatrix} &= \begin{bmatrix} A_{m \times m} & 0_{m \times n}\\  C_{n \times m} & I_{n \times n} \end{bmatrix}  \begin{bmatrix} I_{m \times m} & A^{-1}B_{m \times n}\\  C_{n \times m} & D-CA^{-1}B_{n \times n} \end{bmatrix}\\ &=  \begin{bmatrix} I_{m \times m} & B_{m \times n}\\  0_{n \times m} & D_{n \times n} \end{bmatrix}  \begin{bmatrix} A-BD^{-1}C_{m \times m} & 0_{m \times n}\\  D^{-1}C_{n \times m} & I_{n \times n} \end{bmatrix} \end{align*}
