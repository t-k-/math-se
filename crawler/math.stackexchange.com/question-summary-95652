r
 (x-a)^{2}+(y-b)^{2}=r^{2},\tag{1} 
 (x-a)^{2}+(y-b)^{2}-r^{2}=0, 
 \begin{eqnarray*} (x-a)^{2}+(y-b)^{2}-r^{2} &=&\left( x^{2}-2ax+a^{2}\right) +\left( y^{2}-2by+b^{2}\right) -r^{2} \\ &=&x^{2}+y^{2}-2ax-2by+a^{2}+b^{2}-r^{2}, \end{eqnarray*} 
 x^{2}+y^{2}-2ax-2by+a^{2}+b^{2}-r^{2}=0.\tag{2} 
