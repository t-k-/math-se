Y
 \begin{eqnarray}    1-F_Z(z) &=& \mathbb{P}\left(\min(X,Y) > z\right) = \mathbb{P}\left(X > z, Y>z\right) \\ &=& 1 - \mathbb{P}\left(X\leqslant z\right) - \mathbb{P}\left(Y\leqslant z\right) + \mathbb{P}\left(X\leqslant z, Y\leqslant z\right) \end{eqnarray} 
     F_Z(z) = \mathbb{P}\left(X\leqslant z\right) + \mathbb{P}\left(Y\leqslant z\right) - \mathbb{P}\left(X\leqslant z, Y\leqslant z\right) = F_X(z) + F_Y(z) - F_{X,Y}(z,z) 
M = \min\{A,B\}
\mathbb{P}[M > x] = \mathbb{P}[A > x, B > x] = \mathbb{P}[A > x] \mathbb{P}[B > x]
\begin{split} F_M(x) &= \mathbb{P}[M \leq x] = 1 - \mathbb{P}[M > x] \\        &= 1 - (1-F_A(x))(1-F_B(x)) \\        &= F_A(x) + F_B(x) - F_A(x) F_B(x). \end{split}
f_M(x) = (1-f_A(x))(1-F_B(x)) + (1-F_A(x))(1-f_B(x))
F_U(x) = x
