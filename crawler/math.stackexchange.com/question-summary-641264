2^{nd}
\forall a \forall q [q \notin a \rightarrow \exists l (paral(l,a) \land q \in l) ]
\exists a \exists q [q \notin a \land \lnot \exists l (paral(l,a) \land q \in l)]
\exists a \exists q [q \notin a \land \forall l (q \in l \rightarrow \lnot paral(l,a) )]
\forall a \forall q [q \notin a \rightarrow \exists! l (paral(l,a) \land q \in l) ]
\forall a \forall q [q \notin a \rightarrow ( \exists l (paral(l,a) \land q \in l) \land \forall x ((paral(x,a) \land q \in x) \rightarrow x = l) ) ]
2^{nd}
\forall a \forall q [q \notin a \rightarrow \exists l (paral(l,a) \land q \in l) ]
\exists a \exists q [q \notin a \land \lnot \exists l (paral(l,a) \land q \in l)]
\exists a \exists q [q \notin a \land \forall l (q \in l \rightarrow \lnot paral(l,a) )]
\forall a \forall q [q \notin a \rightarrow \exists! l (paral(l,a) \land q \in l) ]
\forall a \forall q [q \notin a \rightarrow ( \exists l (paral(l,a) \land q \in l) \land \forall x ((paral(x,a) \land q \in x) \rightarrow x = l) ) ]
