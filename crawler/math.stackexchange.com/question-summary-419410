f(x)=f(1/x),
f(x) =  2 \sum_{n=1}^\infty (2n^4\pi^2 x^{9/2} - 3n^2\pi x^{5/2}) e^{-n^2\pi x^2},
f(x) = f_1(x) - f_2(x)
f_1(x) = 4\pi^2 x^{1/2} \sum_{n=1}^\infty (nx)^4  e^{-n^2\pi x^2}
f_2(x) = 6\pi x^{1/2} \sum_{n=1}^\infty (nx)^2  e^{-n^2\pi x^2}
\mathfrak{M}\left(x^4 e^{-\pi x^2}; s\right) =  \frac{1}{2} \frac{\Gamma(s/2+2)}{\pi^{s/2+2}}
\mathfrak{M}\left(x^2 e^{-\pi x^2}; s\right) =  \frac{1}{2} \frac{\Gamma(s/2+1)}{\pi^{s/2+1}}
\mathfrak{M}\left(\sum_{n=1}^\infty (nx)^4 e^{-\pi (nx)^2}; s\right) =  \frac{1}{2} \frac{\Gamma(s/2+2)}{\pi^{s/2+2}}\zeta(s)
\mathfrak{M}\left(\sum_{n=1}^\infty (nx)^2 e^{-\pi (nx)^2}; s\right) =  \frac{1}{2} \frac{\Gamma(s/2+1)}{\pi^{s/2+1}}\zeta(s)
\mathfrak{M}\left(f_1(x); s\right) = 2\pi^2 \frac{\Gamma(s/2+9/4)}{\pi^{s/2+9/4}}\zeta(s+1/2) = 2\frac{\Gamma(s/2+9/4)}{\pi^{s/2+1/4}}\zeta(s+1/2)
\mathfrak{M}\left(f_2(x); s\right) = 3\pi \frac{\Gamma(s/2+5/4)}{\pi^{s/2+5/4}}\zeta(s+1/2) = 3\frac{\Gamma(s/2+5/4)}{\pi^{s/2+1/4}}\zeta(s+1/2).
\mathfrak{M}\left(f_1(x)-f_2(x); s\right) = (2(s/2+5/4)-3) \Gamma(s/2+5/4) \frac{\zeta(s+1/2)}{\pi^{s/2+1/4}} \\ = (s-1/2) \Gamma(s/2+5/4) \frac{\zeta(s+1/2)}{\pi^{s/2+1/4}} \\ = (s-1/2)(s/2+1/4) \Gamma(s/2+1/4) \frac{\zeta(s+1/2)}{\pi^{s/2+1/4}} = \frac{1}{2} (s^2-1/4) \Gamma(s/2+1/4) \frac{\zeta(s+1/2)}{\pi^{s/2+1/4}}.
f(x) = \frac{1}{2\pi i} \int_{5/2-i\infty}^{5/2+i\infty}  \frac{1}{2} (s^2-1/4) \Gamma(s/2+1/4) \frac{\zeta(s+1/2)}{\pi^{s/2+1/4}} \frac{ds}{x^s}.
f(x) = \frac{1}{2\pi i} \int_{-i\infty}^{+i\infty}  \frac{1}{2} (s^2-1/4) \Gamma(s/2+1/4) \frac{\zeta(s+1/2)}{\pi^{s/2+1/4}} \frac{ds}{x^s}.
 \frac{1}{2} (s^2-1/4) \Gamma(s/2+1/4) \frac{\zeta(s+1/2)}{\pi^{s/2+1/4}} = \frac{1}{2} (s^2-1/4) \Gamma(1/4-s/2)  \frac{\zeta(1/2-s)}{\pi^{1/4-s/2}}.
f(1/x) = \frac{1}{2\pi i} \int_{-i\infty}^{+i\infty}  \frac{1}{2} (s^2-1/4) \Gamma(s/2+1/4) \frac{\zeta(s+1/2)}{\pi^{s/2+1/4}} x^s ds.
 f(1/x) = - \frac{1}{2\pi i} \int_{+i\infty}^{-i\infty}  \frac{1}{2} (t^2-1/4) \Gamma(-t/2+1/4) \frac{\zeta(-t+1/2)}{\pi^{-t/2+1/4}} x^{-t} dt.
