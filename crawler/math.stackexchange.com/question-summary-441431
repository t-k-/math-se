 r^4 - a^4 = 0 
 r_{1,2} = \pm a \\ r_{3,4} = \pm ai 
 y_1 = c_1 e^{ax} + c_2 e^{-ax} 
 c_1 \equiv \frac{c_1 + c_2}2 \\ c_2 \equiv \frac {c_1 - c_2}2 
 y_1 = \frac {c_1 + c_2}2 e^{ax} + \frac {c_1 - c_2}2 e^{-ax} = c_1\frac {e^{ax} + e^{-ax}}2 + c_1\frac {e^{ax} - e^{-ax}}2 = c_1 \cosh ax + c_2 \sinh ax 
 y_2 = c_3 e^{axi} + c_4 e^{-axi} 
 c_3 \equiv \frac {c_3 - ic_4}2 \\ c_4 \equiv \frac {c_3 + ic_4}2 
 y_2 = \frac {c_3 - ic_4}2 e^{axi} + \frac {c_3 + ic_4}2 e^{-axi} = c_3 \frac {e^{axi} + e^{-axi}}2 - c_4 i\frac {e^{axi} - e^{-axi}}2 = \\ = c_3 \frac {e^{axi} + e^{-axi}}2 + c_4 \frac {e^{axi} - e^{-axi}}{2i} = c_3 \cos ax + c_4 \sin ax 
 y = c_1 \cosh ax + c_2 \sinh ax + c_3 \cos ax + c_4 \sin ax 
