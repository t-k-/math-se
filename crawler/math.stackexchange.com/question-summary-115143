\begin{eqnarray} u_{n+1}&=&\frac{2u_n^3+2u_n^2+u_n}{2u_n^2+3u_n+1}\\ &\geq& \frac{2/(n+1)^3+2/(n+1)^2+1/(n+1)}{2/(n+1)^2+3/(n+1)+1}\\ &\geq&\frac{1}{n+1}-\frac{1}{(n+1)^2+3(n+1)+2}\\ &\geq& \frac{1}{n+1}-\frac{1}{(n+1)^2+(n+1)}\\ &\geq&\frac{1}{n+2}\end{eqnarray}
