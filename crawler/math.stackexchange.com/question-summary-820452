 \begin{align} \left[1+\frac{n\left(2^{1/n}-1\right)}n\right]^n &=\left[2^{1/n}\right]^n\\ &=2\tag{1} \end{align} 
 \lim_{n\to\infty}\left(1+\frac xn\right)^n=e^x\tag{2} 
 \lim_{n\to\infty}n\left(2^{1/n}-1\right)=\log(2)\tag{3} 
 \begin{align} \lim_{n\to\infty}\left(2-2^{1/n}\right)^n &=\lim_{n\to\infty}\left(1+1-2^{1/n}\right)^n\\ &=\lim_{n\to\infty}\left[1+\frac{n\left(1-2^{1/n}\right)}n\right]^n\\ &=\lim_{n\to\infty}\left[1+\frac{-\log(2)}n\right]^n\\[9pt] &=e^{-\log(2)}\\[12pt] &=\frac12\tag{4} \end{align} 
