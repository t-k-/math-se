\DeclareMathOperator{\mor}{mor}
\mathcal{C}(A, C)
\vec{k}_\bullet : \mor \mathcal{C} \to \mathcal{C}(A, C)
h_\bullet : \mor \mathcal{C} \to \mathcal{C}(A, B)
h_i
\vec{h} : A \to C
\pi_i \circ \vec{h} = h_i
\vec{h} = \vec{k}_j
j
\mor \mathcal{C}
f \ne g
\mathcal{C}
h_i = \begin{cases} f & \text{if } \pi_i \circ \vec{k}_i \ne f \\ g & \text{if } \pi_i \circ \vec{k}_i = f \end{cases}
\pi_j \circ \vec{h} = \begin{cases} f & \text{if } \pi_j \circ \vec{k}_j = \pi_j \circ \vec{h} \ne f\\ g & \text{if } \pi_j \circ \vec{k}_j = \pi_j \circ \vec{h} = f \end{cases}
2^\kappa \gneq \kappa
