 (1+x)(1+y)=2\tag{2} 
 x=\frac{1-y}{1+y}\quad\text{and}\quad\frac{\mathrm{d}x}{1+x^2}=-\frac{\mathrm{d}y}{1+y^2}\tag{3} 
 \log\left(\frac{1+x}{\sqrt2}\right)=-\log\left(\frac{1+y}{\sqrt2}\right)\tag{4} 
 \int_0^1\log\left(\frac{1+x}{\sqrt2}\right)\frac{\mathrm{d}x}{1+x^2}=-\int_0^1\log\left(\frac{1+y}{\sqrt2}\right)\frac{\mathrm{d}y}{1+y^2}\tag{5} 
 \begin{align} \int_0^1\frac{\log(1+x)}{1+x^2}\mathrm{d}x &=\int_0^1\frac{\log(\sqrt2)}{1+x^2}\mathrm{d}x\tag{6} \end{align} 
 \int_0^1\frac{\mathrm{d}x}{1+x^2}=\int_1^\infty\frac{\mathrm{d}x}{1+x^2}\tag{7} 
 \int_0^1\frac{\mathrm{d}x}{1+x^2}=\frac14\int_{-\infty}^\infty\frac{\mathrm{d}x}{1+x^2}\tag{8} 
 \int_{-\infty}^\infty\frac{\mathrm{d}x}{1+x^2}=\int_\gamma\frac{\mathrm{d}z}{1+z^2}\tag{9} 
 \begin{align} \int_0^1\frac{\log(1+x)}{1+x^2}\mathrm{d}x &=\int_0^1\frac{\log(\sqrt2)}{1+x^2}\mathrm{d}x\\ &=\log(\sqrt2)\cdot\frac14\int_{-\infty}^\infty\frac1{1+x^2}\mathrm{d}x\\ &=\log(\sqrt2)\cdot\frac14\cdot\pi\\ &=\frac\pi8\log(2)\tag{10} \end{align} 
