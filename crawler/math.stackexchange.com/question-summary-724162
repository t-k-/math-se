{lnx}
{x=3}
{x=1 , x=2 , x=4 , x=6 }
p
x = 1, 2, 4,
6
a_1 = 0; a_2 = ln(2); a_3 = ln(4)
a_4 = ln(6)
x = 3
ln(3)
 p(1) = ln(1) = 0\\ p(2) = ln(2)\\ p(4) = ln(4) = 2 ln(2) \\ p(6) = ln(6) 
 p(x) =  a_1 \frac{(x-2)(x-4)(x-6)}{(x-1)(x-2)(x-4)(x-6)}+ a_2 \frac{(x-1)(x-4)(x-6)}{(x-1)(x-2)(x-4)(x-6)}+ a_3 \frac{(x-1)(x-2)(x-6)}{(x-1)(x-2)(x-4)(x-6)}+ a_4 \frac{(x-1)(x-2)(x-4)}{(x-1)(x-2)(x-4)(x-6)}, 
 p(x) =  ln(2) \frac{(x-1)(x-4)(x-6)}{(x-1)(x-2)(x-4)(x-6)}+ ln(4) \frac{(x-1)(x-2)(x-6)}{(x-1)(x-2)(x-4)(x-6)}+ ln(6) \frac{(x-1)(x-2)(x-4)}{(x-1)(x-2)(x-4)(x-6)}. 
