AS=S\Lambda
 \begin{align*} AS=S\Lambda\\ A^{-1}AS=A^{-1}S\Lambda\\ S=A^{-1}S\Lambda\\ S^{-1}=(A^{-1}S\Lambda)^{-1}\\ S^{-1}=\Lambda^{-1}S^{-1}(A^{-1})^{-1}\\ S^{-1}=\Lambda^{-1} S^{-1}A\\ \end{align*} 
S^{-1}=\Lambda^{-1} S^{-1}A
S^{-1}=\Lambda S^{-1}A^{-1}
A
 \begin{align*} AS=S\Lambda\\ ASS^{-1}=S\Lambda S^{-1}\\ A=S\Lambda S^{-1}\\ S^{-1}A=S^{-1}S\Lambda S^{-1}\\ S^{-1}AA^{-1}=\Lambda S^{-1}A^{-1}\\ S^{-1}=\Lambda S^{-1}A^{-1}\\ \end{align*} 
S^{-1}=\Lambda^{-1} S^{-1}A
S^{-1}=\Lambda S^{-1}A^{-1}
\Lambda^{-1}
\Lambda^1
