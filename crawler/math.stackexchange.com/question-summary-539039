 \begin{align} \frac{1-\frac{f(x+h)+f(x-h)}{2f(x)}}{h^2} &=-\frac{f(x+h)-2f(x)+f(x-h)}{2f(x)h^2}\\ &=-\frac{\frac12h^2f''(\xi_+)+\frac12h^2f''(\xi_-)}{2f(x)h^2}\\[4pt] &=-\frac{f''(\xi)}{2f(x)}\tag{2} \end{align} 
(3)
 \lim_{h\to0}\frac{1-\frac{f(x+h)+f(x-h)}{2f(x)}}{h^2}=-\frac{f''(x)}{2f(x)}\tag{3} 
 \begin{align} \lim_{h\to0}\frac{1-\frac{f(x+h)+f(x-h)}{2f(x)}}{h} &=\lim_{h\to0}h\frac{1-\frac{f(x+h)+f(x-h)}{2f(x)}}{h^2}\\ &=\lim_{h\to0}h\lim_{h\to0}\frac{1-\frac{f(x+h)+f(x-h)}{2f(x)}}{h^2}\\[4pt] &=0\cdot-\frac{f''(x)}{2f(x)}\\[12pt] &=0\tag{4} \end{align} 
