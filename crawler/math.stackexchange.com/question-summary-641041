 \sum_{0 \leq j \leq d} (-1)^j {d \choose j}=0
 \sum_{0 \leq j \leq \frac{d}{2}} (-1)^j {d \choose j}(d-2j)^\alpha=0, \, \text{  as }\alpha \text { is even and } \alpha<d 
I(d_1,d_2)=0,
d_2,d_1 \in \mathbb{N},
d_2+d_1
d_2>d_1
I(d_1,d_2)=\frac{(-1)^{\frac{d_2-d_1+1}{2}}}{2^{d_1} d_1!}\sum_{k=0}^{ d_1} { d_1 \choose k}\frac{ (2k-d_1)^{d_2+d_1-1}}{ (d_2{+}d_1{-}2k)!! (d_2{-}d_1{+}2k)!!}\\+ \frac{1}{2^{d_2} d_2!}\sum_{  k=0}^{d_2}{ d_2 \choose k}\dfrac{(-1)^k(d_2-2k)^{d_2+d_1-1}}{  \prod_{i=0}^{d_1} {(d_2-d_1-2k+2i)}}. 
 \sum_{0 \leq j \leq d}  {d \choose j}=2^n.
I(d_1,d_2)=\frac{(-1)^{\frac{d_2-d_1+1}{2}}}{2^{d_1+d_2} d_1!}\sum_{k=0}^{ d_1} \sum_{j=0}^{ d_2} { d_2 \choose j} { d_1 \choose k}\frac{ (2k-d_1)^{d_2+d_1-1}}{ (d_2{+}d_1{-}2k)!! (d_2{-}d_1{+}2k)!!}\\+ \frac{1}{2^{d_1+d_2} d_2!}\sum_{  k=0}^{d_2} \sum_{j=0}^{ d_1} { d_1 \choose j} { d_2 \choose k}\dfrac{(-1)^k(d_2-2k)^{d_2+d_1-1}}{  \prod_{i=0}^{d_1} {(d_2-d_1-2k+2i)}}\\ =\frac{1}{2^{d_1+d_2} d_1!d_2!}\sum_{k=0}^{ d_1} \sum_{j=0}^{ d_2} { d_2 \choose j} { d_1 \choose k}(\frac{ (-1)^{\frac{d_2-d_1+1}{2}}(2k-d_1)^{d_2+d_1-1}d_2!}{ (d_2{+}d_1{-}2k)!! (d_2{-}d_1{+}2k)!!}\\+  \dfrac{(-1)^j d_1!(d_2-2j)^{d_2+d_1-1}}{  \prod_{i=0}^{d_1} {(d_2-d_1-2j+2i)}}).
