 f_{Y_1,Y_2}(y_1,y_2)=g(z_1;z_2;\rho),\qquad z_k=(y_k,\mu_k,\sigma_k^2), 
 g(z_1;z_2;\rho)=g(z_2;z_1;\rho). 
 h(z_1)=\int g(z_1;y_2,\mu_2,\sigma_2^2;\rho)\,\mathrm dy_2. 
 f_{Y_2}(y_2)=\int f_{Y_1,Y_2}(y_1,y_2)\,\mathrm dy_1, 
 f_{Y_2}(y_2)=\int g(z_1;z_2;\rho)\,\mathrm dy_1=\int g(z_2;z_1;\rho)\,\mathrm dy_1=\int g(z_2;y_1,\mu_1,\sigma_1^2;\rho)\,\mathrm dy_1=h(z_2), 
 f_{Y_1}(y_1)=h(y_1,\mu_1,\sigma_1^2), 
 f_{Y_2}(y_2)=h(y_2,\mu_2,\sigma_2^2), 
