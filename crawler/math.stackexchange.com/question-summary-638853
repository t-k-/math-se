\displaystyle\left\{ \begin{array}{l} a,b,c>0\\ a+b+c=3 \end{array} \right.
\frac{a^3}{(2a^2+b^2)(2a^2+c^2)}+\frac{b^3}{(2b^2+c^2)(2b^2+a^2)}+\frac{c^3}{(2c^2+a^2)(2c^2+b^2)}\leq\frac{1}{3}
\frac{a^3}{(2a^2+b^2)(2a^2+c^2)}=\frac{1}{a}\frac{1}{(2+\frac{b^2}{a^2})(2+\frac{c^2}{a^2})}
(2+\frac{b^2}{a^2})(2+\frac{c^2}{a^2})=(1+1+\frac{b^2}{a^2})(1+\frac{c^2}{a^2}+1)\ge(1+\frac{c}{a}+\frac{b}{a})^2=\frac{(a+b+c)^2}{a^2}
\frac{1}{(2+\frac{b^2}{a^2})(2+\frac{c^2}{a^2})}\le\frac{a^2}{(a+b+c)^2}
LHS \le \frac{1}{a}\frac{a^2}{(a+b+c)^2}+\frac{1}{b}\frac{b^2}{(a+b+c)^2}+\frac{1}{c}\frac{c^2}{(a+b+c)^2}=\frac{(a+b+c)}{(a+b+c)^2}=\frac{1}{3}
