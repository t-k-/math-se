a=1
b=2
c=2.
(a=1) \;\; AND \;\; (b=1)\;\; AND\;\; (c=2)\tag{1}
\qquad{\bf T}\;\;\; AND \;\;\;\color{red}{\bf F} \;\;\;AND \;\;\;{\bf T}\; = \;\; \color{red}{\bf FALSE}
(a = 1) \wedge (b = 1) \wedge (c = 2)
(1 = 1) \wedge (2 = 1) \wedge (2 = 2)
\text{True} \wedge \text{False} \wedge \text{True} 
