\begin{align} A^{-1}(t) = A(t)^{-1} = & \left[ A(0) + \sum_{c=1}^{\infty} \frac{A^{(c)}(0)}{c!} t^c \right]^{-1}\\ = & \left[ \left( I + \sum_{c=1}^{\infty} \frac{A^{(c)}(0) A^{-1}(0)}{c!} t^c \right) A(0) \right]^{-1}\\ = & A^{-1}(0) + A^{-1}(0) \sum_{s=1}^{\infty}(-1)^s \left( \prod_{i=1}^{s} \frac{A^{(c_i)}(0) A^{-1}(0)}{c_i!}\right) t^{\sum_{i=1}^s c_i} \end{align}
 \frac{d^n A^{-1}}{dt^n}  = n!\sum_{s=1}^n (-1)^s\!\!\!\!\sum_{\stackrel{1 \le c_1,\ldots,c_s \le n}{c_1+\cdots+c_s=n}} \left\{A^{-1} \prod_{i=1}^{s}  \left( \frac{A^{(c_s)}}{c_s!} A^{-1}\right)\right\} 
\begin{align} \frac{d^n A^{-1}}{dx^n} = - A^{-1}A^{(n)}A^{-1} &  + \;\;\sum_{\stackrel{1 \le c_1, c_2 < n}{c_1+c_2 = n}}\frac{n!}{c_1!c_2!}A^{-1}A^{(c_1)} A^{-1} A^{(c_2)} A^{-1}\\   & - \sum_{\stackrel{1 \le c_1, c_2, c_3 < n}{c_1+c_2+c_3=n}}\frac{n!}{c_1!c_2!c_3!}A^{-1}A^{(c_1)} A^{-1} A^{(c_2)}A^{-1} A^{(c_3)} A^{-1}\\   & \;\;\vdots \end{align}
