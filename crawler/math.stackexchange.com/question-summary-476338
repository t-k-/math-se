\text{minimize}\;\;\; 1x_1 + 1.5x_2 + 2x_3 + 1.5x_4 + 4x_5 + 2x_6 + 1x_7 + 3x_8 + 4x_9 + 2x_{10}
\text{subject to}\;\;\;x_1 + x_2 = 10
\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;x_3 + x_4 = 15
\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;x_5 + x_8 = 8
\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;x_6 + x_9 = 14
\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;x_7 + x_{10} = 3
\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;\;x_1 \ge 0, x_2 \ge 0, \cdots, x_{10} \ge 0.
