\dot{\theta}_1=w_1
\dot{\theta}_2=w_2
\dot{w}_1=-L_1\sin\theta_1+\frac{m_2}{m_1}\cos\theta_2\sin(\theta_2-\theta_1)
\dot{w}_2=-L_2\sin\theta_2.
 E(\theta_1,\theta_2,w_1,w_2)=T+U 
 T=\frac{1}{2}((m_1+m_2)L_1^2+w_1^2+m_2L_2^2w_2^2)+m_2L_1L_2\cos(\theta_1-\theta_2)w_1w_2 
 U=m_1gL_1(1-\cos(\theta_1)+m_2gL_2(1-\cos\theta_2).
 \frac{\partial}{\partial\theta_1}E\cdot w_1+\frac{\partial}{\partial\theta_2}E\cdot w_2+\frac{\partial}{\partial w_1}E\cdot (-L_1\sin\theta_1+\frac{m_2}{m_1}\cos\theta_2\sin(\theta_2-\theta_1))+\frac{\partial}{\partial w_2}E\cdot (-L_2\sin\theta_2)=0,
0
