\eqalign{   & L(f,P) = \sum\limits_{i = 1}^n {{{\left( {i - 1} \right)}^2}\frac{{{a^2}}}{{{n^2}}}} \frac{a}{n} = \frac{{{a^3}}}{{{n^3}}}\sum\limits_{i = 1}^n {{{\left( {i - 1} \right)}^2}}   \cr    & U(f,P) = \sum\limits_{i = 1}^n {{i^2}\frac{{{a^2}}}{{{n^2}}}} \frac{a}{n} = \frac{{{a^3}}}{{{n^3}}}\sum\limits_{i = 1}^n {{i^2}}  \cr} 
\sum\limits_{i = 1}^n {{i^2}}  = \frac{{n\left( {n + 1} \right)\left( {2n + 1} \right)}}{6}
\eqalign{   & L(f,P) = \frac{{{a^3}}}{{{n^3}}}\sum\limits_{i = 0}^{n - 1} {{i^2}}  = \frac{{{a^3}}}{3}\frac{{n\left( {n - 1} \right)\left( {2n - 1} \right)}}{{2{n^3}}}  \cr    & U(f,P) = \frac{{{a^3}}}{{{n^3}}}\sum\limits_{i = 1}^n {{i^2}}  = \frac{{{a^3}}}{3}\frac{{n\left( {n + 1} \right)\left( {2n + 1} \right)}}{{2{n^3}}} \cr} 
\eqalign{   & L(f,P) \to \frac{{{a^3}}}{3}  \cr    & U(f,P) \to \frac{{{a^3}}}{3} \cr} 
\int\limits_a^b {{x^2}dx}  = \int\limits_0^b {{x^2}dx}  - \int\limits_0^a {{x^2}dx}  = \frac{{{b^3}}}{3} - \frac{{{a^3}}}{3}
