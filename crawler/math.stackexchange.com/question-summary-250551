\begin{align} \psi1 = &\forall u  \neg L(u,u) \\  &\wedge \forall u\forall v (L(u,v) \Rightarrow \neg L(v,u)) \\  & \wedge \forall u\forall v \forall w(L(u,v) \wedge L(v,w) \Rightarrow L(u,w))  \end{align} 
\begin{align} \psi2 = &\forall u\forall v ((L(u,v) \ \wedge \forall w(\neg L(u,w) \vee \neg L(w,v))) \\ & \Rightarrow E(u,v)). \end{align} 
\begin{align} \psi3 = &(\forall u\ \neg L(u,x)) \wedge (\forall v\ \neg L(y,v)) \ \wedge L(x,y) \end{align} 
\phi(x,y) =\exists L (x=y) \vee (\psi1 \wedge \psi2 \wedge \psi3))
\phi(x,y) =(x=y) \vee (\psi1 \wedge \psi2 \wedge \psi3))
\forall x \forall y \ p
s4 = s1 \wedge \ s2 \wedge \ s3
