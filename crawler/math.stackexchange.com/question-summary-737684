f:X\rightarrow Y
A, B \subseteq Z
f^{-1}
x\in f^{-1}(A\cup B)
f(x)\in A\cup B
f(x)\in A
f(x)\in B
x\in f^{-1}(A)
x\in f^{-1}(B)
x\in f^{-1}(A)\cup f^{-1}(B)
f^{-1}(A\cup B)\subset f^{-1}(A)\cup f^{-1}(B).
x\in f^{-1}(A)\cup f^{-1}(B)
x\in f^{-1}(A)
x\in f^{-1}(B)
f(x)\in A
f(x)\in B
f(x)\in A\cup B
x\in f^{-1}(A\cup B)
f^{-1}(A)\cup f^{-1}(B)\subset f^{-1}(A\cup B).
 \boxed{f^{-1}(A\cup B) = f^{-1}(A)\cup f^{-1}(B)} 
