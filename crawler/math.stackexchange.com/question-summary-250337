 \eqalign{R_{a_2,\rho_2}(R_{a_1,\rho_1}(z)) &= a_2 + \frac{\rho_2^2}{\overline{R_{a_1,\rho_1}(z)}-a_2}\cr &=a_2 + \dfrac{\rho_2^2}{\overline{a_1 + \frac{\rho_1^2}{\overline{z - a_1}} - a_2}}\cr &= a_2 + \dfrac{\rho_2^2(z - a_1)}{(\overline{a_1} - \overline{a_2})(z - a_1) + \rho_1^2}}
