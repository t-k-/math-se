 f(x)  = x^2 -x+y+i(y^2-5y-x)
u(x,y) = x^2-x+y
v(x,y) = y^2-5y-x
u_x = 2x-1
u_y = 1
v_x= -1
v_y= 2y-5
u_y = -v_x \Rightarrow 1 = -(-1) \Rightarrow 1 = 1
u_x \neq  v_y\Rightarrow y = x+2
h(x,y)=U(x,y)+iV(x,y)
\partial_{x}(U(x,y))=\partial_{y}(V(x,y))
\partial_{y}(U(x,y))=-\partial_{x}(V(x,y))
h(x,y)=U(x,y)+iV(x,y)=f(z)=f(x+iy)
h(x,y)=e^{x}\cos(y)+ie^{x}\sin(y)
U(x,y)=e^{x}\cos(y)
V(x,y)=e^{x}\sin(y)
\partial_{x}(U(x,y))=e^{x}\cos(y)
\partial_{y}(U(x,y))=-e^{x}\sin(y)
\partial_{x}(V(x,y))=e^{x}\sin(y)
\partial_{y}(V(x,y))=e^{x}\cos(y)
\partial_{x}(U(x,y))=\partial_{y}(V(x,y))
\partial_{y}(U(x,y))=-\partial_{x}(V(x,y))
h(x,y)
h(x,y)=f(z)=f(x+iy)
h(x,y)=e^{x}\cos(y)+ie^{x}\sin(y)=e^{x}(\cos(y)+i\sin(y))=e^{x}e^{iy}=e^{x+iy}=e^{z}
h(x,y)=f(z)=e^{z}
