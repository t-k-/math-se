\ds{\int_{0}^{1}{\ln\pars{x^{2} - x + 1} \over x^{2} - x}\,\dd x:\ {\large ?}}
\ds{x^{2} - x + 1 = 0}
\ds{\quad x_{\rm r} \equiv {1 + \root{3}\ic \over 2}\quad}
\ds{\quad{1 \over x_{\rm r}} = x_{\rm r}^{*}}
\ds{{\rm Li}_{1}\pars{z} = -\ln\pars{1 - z}}
\ds{{\rm Li}_{2}\pars{z} = \int_{0}^{z}{{\rm Li}_{1}\pars{t} \over t}\,\dd t}
 \color{#66f}{\large% \int_{0}^{1}{\ln\pars{x^{2} - x + 1} \over x^{2} - x}\,\dd x = {\pi^{2} \over 9}} 
