x=(x_1,x_2)
[x]=\sqrt{x_1^2+x_1x_2+x_2^2}
y=(y_1,y_2)
[x+y]\le[x]+[y]
4x_1^2y_1^2-6x_1x_2y_1y_2+4x_2^2y_2^2-x_1^2x_2^2-y_1^2y_2^2\  (\text{this must be bigger than }0)
u_1,u_2,v_1,v_2
[x]=\sqrt{(x_1+{\textstyle\frac{1}{2}}x_2)^2+({\textstyle\frac{\sqrt3}{2}}x_2)^2}\ .
\sqrt{(u_1+v_1)^2+(u_2+v_2)^2}\le\sqrt{u_1^2+u_2^2}+\sqrt{v_1^2+v_2^2}
u_1=x_1+{\textstyle\frac{1}{2}}x_2\,,\quad u_2={\textstyle\frac{\sqrt3}{2}}x_2\,,\quad   v_1=y_1+{\textstyle\frac{1}{2}}y_2\,,\quad v_2={\textstyle\frac{\sqrt3}{2}}y_2
