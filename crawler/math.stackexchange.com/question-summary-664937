 \begin{align} f'(x) &= \frac{\sum_i{p_i(x,f(x),g(x))\mathrm{e}^{i f(x)}}}{\sum_j{q_j(x,f(x),g(x))\mathrm{e}^{j f(x)}}} \\\ g'(x) &= \frac{\sum_i{r_i(x,f(x),g(x))\mathrm{e}^{i f(x)}}}{\sum_j{s_j(x,f(x),g(x))\mathrm{e}^{j f(x)}}} \end{align}, 
