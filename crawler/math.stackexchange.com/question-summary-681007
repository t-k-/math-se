a={{{(a_n)_n}_\in}_b}_N
b={{{(b_n)_n}_\in}_b}_N
m\in N
n_0 \in N
|a_n-b_n|<1/m
n \ge n_0
a_n == b_n
==
a_n == a_n
n_0=1
m
a_n == b_n
b_n == a_n
|a_n - b_n| = |b_n - a_n|
a_n == b_n
b_n == c_n
a_n == c_n
n_0
|a_n - b_n| < \frac{1}{2m}
n_1
|b_n - c_n| < \frac{1}{2m}
|a_n - c_n| \leq |a_n - b_n| + |b_n - c_n| < \frac{1}{m}
m > \max(n_0,n_1)
