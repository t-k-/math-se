 A^{n}-B^{n}=(A-B)(A^{n-1}+A^{n-2}B+\ldots +B^{n-1}). 
 \begin{equation} A^{n}=B^{n}\Leftrightarrow A^{n}-B^{n}=0  \tag{1} \end{equation}
 \begin{equation} A-B=0\Leftrightarrow A=B  \tag{2} \end{equation}
 \begin{equation} A^{n-1}+A^{n-2}B+\ldots +B^{n-1}=0  \tag{3} \end{equation}
