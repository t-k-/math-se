\eqalign{   & \left[ {{\varphi ^2}\left( {{D^2}} \right) - {D^2}{\xi ^2}\left( {{D^2}} \right)} \right]y = \left[ {\varphi \left( {{D^2}} \right) - D\xi \left( {{D^2}} \right)} \right]\sin ax  \cr    & \left[ {{\varphi ^2}\left( {{D^2}} \right) - {D^2}{\xi ^2}\left( {{D^2}} \right)} \right]y = \varphi \left( { - {a^2}} \right)\sin ax - a\xi \left( { - {a^2}} \right)\cos ax  \cr    & \left[ {{\varphi ^2}\left( {{D^2}} \right) - {D^2}{\xi ^2}\left( {{D^2}} \right)} \right]y = \alpha \sin ax + \beta \cos ax  \cr    & \Phi \left( {{D^2}} \right)y = \alpha \sin ax + \beta \cos ax \cr} 
\Phi \left( { - {a^2}} \right) \ne 0
\frac{{\sin ax}}{{\Phi \left( { - {a^2}} \right)}} = \frac{{\sin ax}}{{\Phi \left( {{D^2}} \right)}}
\eqalign{   & y = \frac{{\alpha \sin ax + \beta \cos ax}}{{\Phi \left( { - {a^2}} \right)}}  \cr    & y = \frac{{\varphi \left( { - {a^2}} \right)\sin ax - a\xi \left( { - {a^2}} \right)\cos ax}}{{{\varphi ^2}\left( { - {a^2}} \right) + {a^2}{\xi ^2}\left( { - {a^2}} \right)}} \cr} 
