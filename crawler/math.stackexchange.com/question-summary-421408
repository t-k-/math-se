a,b,c > 0
a^{\log_b(c)} = c^{\log_b(a)}
\log
e^{\log_ea}=a
\log_bm=\frac{\log m}{\log b}
a^{\log_bc}=(e^{\log_ea})^{\log_bc}=e^{\log_ea \cdot \log_bc}=e^{\log_ec\cdot \log_ba}=(e^{\log_ec})^{\log_ba}=c^{\log_ba}
a^b=c \iff log_{a}c=b
a^{\log_b(c)} =x \to log_{a}x =\log_b(c)=log_{b}a\cdot log_{a}c\to\frac{log_{a}x}{log_{a}c}=log_{b}a \to log_{c}x=log_{b}a \to x=c^{\log_b(a)}
a^{\log_b(c)}=(b^{\log_b(a)})^{\log_b(c)}=b^{\log_b(a)\log_b(c)}
c^{\log_b(a)}=(b^{\log_b(c)})^{\log_b(a)}=b^{\log_b(c)\log_b(a)}
