X=[x_1, x_2,...x_n] Y = [y_1, y_2,...y_n]
x_i, y_i
P(x=1) = .5
 P(x=2) = .5 
P(y=1)=.5
P(y=2)=.5
E[X.Y]
E[X.Y] = [E[x_1y_1], E[x_2y_2],.....,E[x_ny_n]]
E[X.Y] = E[\sum_{1}^{n} (x_{i}y_{i})]=\sum_{1}^{n} E[x_{i}y_{i}]=\sum_{1}^{n}2.25=2.25n
X=[x_{1},x_{2},...x_{n}]
Y=[y_{1},y_{2},...y_{n}]
X.Y = \sum_{1}^{n} (x_{i}y_{i}) 
E[X.Y] = E[\sum_{1}^{n} (x_{i}y_{i})]
E[a+b] = E[a]+E[b]
E[\sum_{1}^{n} (x_{i}y_{i})]=\sum_{1}^{n}E[x_{i}y_{i}]
z_{i} = x_{i}y_{i}
z_{i}
z_{i} = \left\{\begin{matrix} 1 & .25 \\ 2 & .5\\  4 & .25 \end{matrix}\right.
E[z_{i}]=\sum_{1}^{3}z_{i}=2.25=E[x_{i}y_{i}]
\sum_{1}^{n}E[x_{i}y_{i}]=\sum_{1}^{n}2.25=2.25n
E[x_1y_1+x_2y_2+...+x_ny_n] = nE[x_iy_i]
