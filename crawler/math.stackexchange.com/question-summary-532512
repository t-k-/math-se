\lbrace f_k \rbrace
\mathbb R_e
E\subset \mathbb R^n
\limsup f_k = \lim\limits_{j\rightarrow \infty} g_j
g_j = \sup\limits_{k\geq j } f_k
f_k \leq g_j \Rightarrow \int_E f_k \leq \int_E g_j 
\sup\limits_{k\geq j }\int_E f_k \leq \int_E g_j 
\lim\limits_{j\rightarrow \infty}\sup\limits_{k\geq j }\int_E f_k \leq \lim\limits_{j\rightarrow \infty}\int_E g_j 
\limsup\limits_{j\rightarrow \infty}\int_E f_k \leq \liminf\limits_{j\rightarrow \infty}\int_E g_j 
\liminf
f_k \leq g_j \Rightarrow 0 \leq g_j - f_k
\int_E 0 \leq \int_E g_j - \int_E f_k
 \liminf\int_E g_j - \liminf\int_E f_k
 \int_E \liminf g_j - \liminf\int_E f_k \leq \liminf\int_E g_j - \liminf\int_E f_k
 \int_E \liminf g_j - \limsup\int_E f_k \leq \int_E \liminf g_j - \liminf\int_E f_k
\int_E \lim\limits_{j\rightarrow\infty} g_j - \limsup\int_E f_k = \int_E \liminf g_j - \limsup\int_E f_k
0 \leq \int_E \limsup\limits_{j\rightarrow\infty} f_k - \limsup\int_E f_k 
\limsup\limits_{j\rightarrow\infty}\int_E f_k \leq \int_E \limsup\limits_{j\rightarrow\infty} f_k  
