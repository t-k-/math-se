\begin{align*} f(x) &= \log_3(x + \sqrt{x^2 - 1})\\   f'(x) &= \frac{1}{\ln3}\frac{1}{x + \sqrt{x^2 - 1}}\left(1 + \frac{1}{2}\frac{1}{\sqrt{x^2 - 1}}2x\right)\\   f'(x) &= \frac{1}{\ln3}\left(\frac{1}{x + \sqrt{x^2 - 1}} + \frac{x}{(x + \sqrt{x^2 -1})\sqrt{x^2 -1}}\right)\\   f'(x) &=\frac{1}{\ln3}\left(\frac{x + \sqrt{x^2 -1}}{(x + \sqrt{x^2 -1})\sqrt{x^2 -1}}\right)\\ f'(x) &=\frac{1}{\ln3}\frac{1}{\sqrt{x^2 -1}} \end{align*}
