n = 1
1^3 = 1^2
1^3 + 2^3 + 3^3 + \cdots + n^3 = (1 + 2 + 3 + \cdots + n)^2
n \in \mathbb N
\displaystyle (1 + 2 + 3 + \cdots + n) = \frac{n(n+1)}{2}
\displaystyle 1^3 + 2^3 + 3^3 + \cdots + n^3 = \bigg(\frac{n(n+1)}{2}\bigg)^2
\displaystyle 1^3 + 2^3 + 3^3 + \cdots + n^3 + (n + 1)^3 =  \bigg(\frac{n(n+1)}{2}\bigg)^2 + (n+1)^3 = \frac{n^2 (n+1)^2 + 4(n+1)^3}{4} = \bigg( \frac{(n+1)(n+2)}{2} \bigg)^2
