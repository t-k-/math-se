K=2k+1<M=2m+1
H^\ast (S^K;{\bf Z}) = \wedge_{\bf Z}[a]
 H^\ast (S^M;{\bf Z}) =\wedge_{\bf Z}[b]
    H^\ast (S^K;{\bf Z})\otimes_{{\bf Z}} H^\ast (S^M;{\bf Z}) =H^\ast (S^K\times S^M;{\bf Z}) =\wedge_{\bf Z} [a,b]
ab=ba \in H^\ast (S^K;{\bf Z})\otimes_{{\bf Z}} H^\ast (S^M;{\bf Z})
 ab=(-1)^{KM}b\cup a=-ba\in H^\ast (S^K\times S^M;{\bf Z})  =\wedge_{\bf Z} [a,b]
