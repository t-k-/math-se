\begin{array}{l}  \cos \left(A_2 k\right)\to \sin \left(A_2 k\right) \cot \left(A_1 k\right) \\  \cos \left(A_1 k\right)\to 0 \\  \sin \left(A_1 k\right)\to 0 \\  \sin \left(A_2 k\right)\to 0 \\  \sin \left(A_1 k\right)\to 0 \end{array}
