 \begin{eqnarray}   \mathcal{I} &=& \int_0^\infty \sin(y^2) \mathrm{d} \left( \frac{-1}{y} \right) =     \left.-\frac{\sin(y^2)}{y} \right|_0^\infty + \int_0^\infty \frac{2 y \cos(y^2)}{y}  \mathrm{d} y \\    &=& 0 + 2 \int_0^\infty \cos(y^2) \mathrm{d}y = 2 \operatorname{Re}\left( \int_0^\infty \mathrm{e}^{i y^2} \mathrm{d} y\right) = 2 \operatorname{Re}\left( \mathrm{e}^{i \pi/4} \int_0^\infty \mathrm{e}^{-y^2} \mathrm{d} y\right) = \sqrt{\frac{\pi}{2}}  \end{eqnarray}  
