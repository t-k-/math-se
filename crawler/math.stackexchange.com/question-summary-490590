a_n = \frac{4}{n+3}
a_n = \frac{2n}{n+1}
a_n = \frac{n^2 + 2}{2n^2+1}
a_n = 2n + 3
a_n = \frac{ (-1)^n n}{n+2}
a_n = \frac{2n + n}{2n + 1}
\displaystyle a_n\leq \frac{4}{n}\leq\cdots
\displaystyle a_n\leq \frac{2n}{n}\leq\cdots 
\displaystyle a_n\leq \frac{3n^2}{2n^2}\leq \cdots
\displaystyle a_M=2M+3>M
\displaystyle |a_n|=\frac{n}{n+2}\leq \frac{n}{n}=1\leq\cdots
\displaystyle a_n=\frac{3n}{n+1}
