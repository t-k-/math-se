\begin{align} \color{blue}{\bf (A_1 \cup A_2)\setminus (B_1 \cap B_2)} & = (A_1\cup A_2) \cap (B_1 \cap B_2)^\complement \\ \\  & = (A_1 \cup A_2) \cap (B_1^\complement \cup B_2^\complement)\\ \\ & = (A_1\cap B_1^\complement)\cup (A_1\cap B_2^\complement)\cup (A_2\cap B_1^\complement)\cup (A_2\cap B_2^\complement)\\ \\ &\color{blue}{\bf = (A_1\setminus B_1) \cup (A_1 \setminus B_2) \cup (A_2\setminus B_1) \cup (A_2\setminus B_2)}\end{align}
