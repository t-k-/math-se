f_{(U_1,U_2)}(u_1,u_2)=\begin{cases} 1/2& -u_1<u_2<u_1 \text{ and } u_1 - 2 < u_2 < 2 - u_1 \text{ and } 0 < u_1 <2\\ 0& \text{otherwise}\end{cases}
f_{(U_1)}(u_1) = \begin{cases} u_1 & 0<u_1<1\\2-u_1 &1<u_1<2 \\0 & \text{otherwise}\end{cases}
f_{(U_2)}(u_2) = \begin{cases} 1+u_2 & -1<u_2<0\\1-u_2 &0<u_2<1 \\0 & \text{otherwise}\end{cases}
