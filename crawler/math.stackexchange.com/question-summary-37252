\begin{align} \langle x, y \rangle_1 &=\frac{1}{4} \left(\|x + y \|^2 - \|x-y\|^2 +i\|x+iy\|^2 - i\|x-iy\|^2\right) \ \forall\ x,y \in V \ \\ \langle x, y \rangle_2 &=\frac{1}{4} \left(\|x + y \|^2 - \|x-y\|^2 +i\|x+iy\|^2 - i\|x-iy\|^2\right) \ \forall\ x,y \in V \end{align}
