\exists x S(x)
\exists y Q(y)
\exists y (Q(y) \land E(y))
\exists z (Q(z) \land H(z))
\exists x \exists y [ S(x) \land Q(y) \land (E(y) \rightarrow S(x,y)) \land (H(y) \rightarrow \lnot S(x,y))]
\exists x (S(x) \land S(x,E) \land \lnot S(x,H))
∃x(student(x) \land solve(x, easy) \land \lnot solve(x, hard))
∃x(student(x) \rightarrow (solve(x, easy) \land \lnot solve(x, hard)))
