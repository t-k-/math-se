 \begin{align*} f(x,y) &= x^2 +2xy + y^2 \\f'_x(x,y) &= 2x+2y \\f'_y(x,y) &= 2x + 2y \end{align*} 
 \begin{align*} f(x,y) &= x^3e^{-y} + y^3 \\f'_x(x,y) &= 3x^2e^{-y} \\f'_y(x,y) &= -x^3e^{-y} +3y^2 \end{align*} 
 \begin{align*} f(x,y) &= \frac{x^2y^3}{\sqrt{x+y}} \\f'_x(x,y) &= \frac{\sqrt{x+y} \times 2xy^3 - \frac{x^2y^3}{2\sqrt{x+y}}}{x+y} \\&= \frac{2xy^3(x+y) - \frac{x^2y^3}{2}}{(x+y)\sqrt{x+y}} \\f'_y(x,y) &= \frac{\sqrt{x+y} \times 3x^2y^2 - \frac{x^2y^3}{2\sqrt{x+y}}}{x+y} \\&= \frac{ 3x^2y^2(x+y) - \frac{x^2y^3}{2}}{(x+y)\sqrt{x+y}} \end{align*} 
\sqrt{x+y}
f_y'
y\cdot (x^2e^{xy})_y'=yx^3e^{xy}
