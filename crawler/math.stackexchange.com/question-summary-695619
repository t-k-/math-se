 \tag{1} I~:=~\int_{-\infty}^{\infty} \! dx~\exp \left(\alpha x^4+\beta x^3+\gamma x^2 +\delta x+\epsilon \right), \qquad \alpha ~<~0,
 a~:=~\sqrt[4]{-\alpha}>0, \qquad b~:=~\frac{\beta}{a^3}, \qquad c~:=~\frac{\gamma}{a^2}, \qquad d~:=~\frac{\gamma}{a}, 
 \tag{2} y~:=~ax, \qquad t~:=~y^4. 
 I~=~\frac{1}{a}\int_{-\infty}^{\infty} \! dy~\exp \left(- y^4+by^3+cy^2 +dy\right)
~=~\frac{1}{a}\sum_{n,m,p\in \mathbb{N}_0}  \frac{b^n}{n!}\frac{c^m}{m!}\frac{d^p}{p!}\int_{-\infty}^{\infty} \! dy~y^{3n+2m+p}e^{- y^4} 
~\stackrel{(4)}{=}~ \frac{1}{a}\sum_{k,n,m,p\in \mathbb{N}_0} \frac{b^n}{n!}\frac{c^m}{m!}\frac{d^p}{p!} ~\delta_{2k,3n+2m+p} ~\frac{1}{2} \Gamma(\frac{2k+1}{4}) 
\tag{3}~=~ \frac{1}{2}  \sum^{n\equiv p \pmod 2}_{n,m,p\in \mathbb{N}_0}  \frac{\beta^n}{n!}\frac{\gamma^m}{m!}\frac{\delta^p}{p!} \frac{\Gamma(\frac{3n+2m+p+1}{4})}{a^{3n+2m+p+1}},
\tag{4}  2\int_{0}^\infty \! dy~y^{2k} e^{- y^4} ~=~\frac{1}{2}\int_{0}^{\infty}  \! dt~t^{\frac{2k-3}{4}}e^{-t}~=~\frac{1}{2}\Gamma(\frac{2k+1}{4}).
