 y_1[n] = IDFT_N \{|X[k]|^2\} 
 y_2[n] = IDFT_N\{\Re X[k]\} 
 y_3[n] = IDFT_N\{ X[(-k)\mod N]\} 
 y_4[n] = IDFT_N\{(-1)^k X[k]\} 
 y_5[n] = IDFT_N\{X^*[k]\} 
x[n] \leftrightarrow X[k]
a
y[n] \leftrightarrow Y[k]
y[n]= a \; x[n]
Y[k] = a X[k]
a \; x[n] \leftrightarrow a \;X[k]
a X[k]
a
x[n] * y[n] \leftrightarrow X[k] Y[k]
x[n] * x[-n] \leftrightarrow |X[k]|^2
