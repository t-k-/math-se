\int \frac{\ln(1+t)}{t^2} \ \mathrm{d}t = -\frac1{t}\ln(1+t) - \int -\frac1{t}\cdot\frac1{1+t} \  \mathrm{d}t = -\frac1{t}\ln(1+t) + \int \frac1{t}\cdot\frac1{1+t} \  \mathrm{d}t
 \int \frac1{t(t+1)} \ \mathrm{d}t = \int \frac1{t}-\frac1{t+1} \ \mathrm{d}t = \ln t - \ln (t+1) + C 
 \int \frac{\ln(1+e^x)}{e^x} \ \mathrm{d}x= x - \ln(1+e^x) - \frac{\ln(1+e^x)}{e^x} + C
 \int\frac{\ln(1+e^x)}{e^x} \; dx = \overbrace{\int u\;dv = uv - \int v\;du}^\text{This is integration by parts.} 
 \begin{align} u & = \ln(1+e^x), \qquad dv =\frac{dx}{e^x}  \\  \\  \\ du &= \frac{e^x}{1+e^x}\;dx, \qquad  v = \frac{-1}{e^x} \end{align} 
 \begin{align} & {} \qquad uv - \int v\;du = \frac{\ln(1+e^x)}{e^x} - \int \frac{-dx}{1+e^x} = \frac{\ln(1+e^x)}{e^x} - \int \frac{-e^{-x}\;dx}{e^{-x}+1} \\  \\  \\ & = \frac{\ln(1+e^x)}{e^x} + \int \frac{e^{-x}\;dx}{e^{-x}+1} = \frac{\ln(1+e^x)}{e^x} + \int \frac{dw}{w+1} \\  \\  \\ & = \frac{\ln(1+e^x)}{e^x} + \ln(w+1) + C = \frac{\ln(1+e^x)}{e^x} + \ln(e^{-x}+1) + C. \end{align} 
 \ln(e^{-x} + 1) = \ln\Big( e^{-x}(1+e^x) \Big) = \ln(e^{-x}) + \ln(1+e^x) = -x + \ln(1+e^x), 
